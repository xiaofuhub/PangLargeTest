 column=(type=long updatewhereclause=yes name=data_view_screen_exp_field dbname="data_view_screen.exp_field" dbalias="sd.exp_field" )
 retrieve="select sa.table_name, 
         sf.field_name, 
         sf.field_name_allias,
         sd.exp_field 
from sys_tables sa,
	    sys_fields sf, 
        data_view_screen sd
where sd.data_view_id =1
  and sa.table_id = sf.table_id 
  and sd.table_id = sa.table_id
  and sd.screen_id = :al_screen_id
  and sf.field_id = :al_field_id

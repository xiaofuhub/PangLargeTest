 retrieve="SELECT DISTINCT  verif_info.rec_id ,
           verif_info.prac_id ,
           verif_info.first_sent ,
           verif_info.last_sent ,
           verif_info.number_sent ,
           verif_info.date_recieved ,
           verif_info.user_name ,
           verif_info.source ,
           verif_info.response_code ,
           verif_info.active_status ,
           verif_info.verification_method ,
           verif_info.expiration_date ,
           verif_info.verification_id ,
           verif_info.reference_value ,
           verif_info.screen_id ,
           data_view_screen.screen_name ,
           verif_info.address_lookup_code ,
           verif_info.seq_no ,
           address_lookup.phone ,
           address_lookup.fax ,
           address_lookup.email_address ,
           address_lookup.web_address ,
           address_lookup.fee ,
           v_full_name.full_name ,
           verif_info.facility_id ,
           verif_info.priority_user ,
           verif_info.priority ,
           verif_info.exp_credential_flag ,
           pd_affil_stat.verifying_facility ,
           facility.facility_name ,
           verif_info.doc_id ,
           verif_info.print_flag ,
           verif_info.notes ,
           v_full_name.prac_id ,
           selected = 1,
           verif_info.direct_parent_facility_link ,
           data_view_screen.screen_alias ,
           verif_info.verification_picture_flag ,
           verif_info.wf_step ,
           verif_info.due_date ,
           verif_info.letter_id ,
           verif_info.wf_id ,
           verif_info.Wf_action_type_id ,
			  verif_info.wf_complete_flag ,
	  		  verif_info.data_status,
           verif_info.create_date,  
           verif_info.create_user,   
           verif_info.mod_date,   
           verif_info.mod_user   ,
         verif_info.fee,
		verif_info.image_stamp,
		verif_info.image_stamp_user,
		verif_info.image_stamp_site, 
		icred_settings.set_75
        FROM {oj verif_info LEFT OUTER JOIN address_lookup ON verif_info.address_lookup_code = address_lookup.lookup_code} ,
           data_view_screen ,
           pd_affil_stat ,
           v_full_name ,
           facility ,
		icred_settings   
        WHERE ( verif_info.screen_id = data_view_screen.screen_id ) and 
         ( verif_info.facility_id = pd_affil_stat.verifying_facility ) and 
         ( verif_info.prac_id = v_full_name.prac_id ) and         
	 ( verif_info.prac_id = pd_affil_stat.prac_id ) and  
        ( pd_affil_stat.verifying_facility = facility.facility_id ) and        
         ( data_view_screen.data_view_id = 1 ) and        
		( verif_info.active_status = 1 ) and 
         ( pd_affil_stat.active_status in (1, 4)) and    
		( verif_info.response_code IS NULL ) 


  " update="verif_info" updatewhere=0 updatekeyinplace=no  sort="v_full_name_full_name A doc_id A screen_name A " )

 column=(type=long updatewhereclause=yes name=screen_id dbname="data_view_screen.screen_id" )
 column=(type=char(30) updatewhereclause=yes name=code_lookup_code dbname="code_lookup.code" )
 column=(type=char(17) updatewhereclause=yes name=cdays_to_exp dbname="days_to_exp" )
 retrieve="SELECT DISTINCT verif_info.rec_id,
		verif_info.prac_id,
		verif_info.reference_value,
 		verif_info.priority_user,
	   data_view_screen.screen_name,
	   data_view_screen.screen_id,
      code_lookup.code,
      days_to_exp = (case when verif_info.print_flag = 0 then 'Awaiting Response' when verif_info.print_flag = 1 then ' Need Processed' end)
 FROM code_lookup,
      data_view_screen,
      verif_info ,
	   pd_affil_stat,
		security_user_facilities
 WHERE ( verif_info.active_status = 1 ) and
	( verif_info.response_code IS NULL) and
	( verif_info.print_flag = 0 or verif_info.print_flag = 1) and
	( security_user_facilities.access_rights = 1 ) and
	( security_user_facilities.user_id = :user_id) and
	( pd_affil_stat.active_status = 1 or pd_affil_stat.active_status = 4) and
	( data_view_screen.data_view_id = 1 ) and
	( verif_info.screen_id = data_view_screen.screen_id ) and 
   ( verif_info.facility_id = pd_affil_stat.verifying_facility and data_view_screen.screen_id <> 1 or verif_info.facility_id = pd_affil_stat.parent_facility_id and data_view_screen.screen_id = 1) and 
	( security_user_facilities.facility_id = pd_affil_stat.parent_facility_id ) and
	( verif_info.prac_id = pd_affil_stat.prac_id ) and
	( verif_info.verification_method = code_lookup.lookup_code )
" arguments=(("user_id", string)) )

 column=(type=long updatewhereclause=yes key=yes name=verif_info_seq_no dbname="verif_info.seq_no" dbalias=".seq_no" )
 column=(type=long updatewhereclause=yes key=yes name=verif_info_facility_id dbname="verif_info.facility_id" dbalias=".facility_id" )
 column=(type=long update=yes updatewhereclause=yes name=print_flag dbname="verif_info.print_flag" dbalias=".print_flag" )
 column=(type=long updatewhereclause=yes name=facility_ver_rules_ver_letter_resend_day dbname="facility_ver_rules.ver_letter_resend_days" dbalias=".ver_letter_resend_days" )
 column=(type=datetime updatewhereclause=yes name=verif_info_last_sent dbname="verif_info.last_sent" dbalias=".last_sent" )
 column=(type=datetime updatewhereclause=yes name=compute_0008 dbname="compute_0008" )
 column=(type=datetime update=yes updatewhereclause=yes name=create_date dbname="verif_info.create_date" dbalias=".create_date" )
 column=(type=char(10) update=yes updatewhereclause=yes name=create_user dbname="verif_info.create_user" dbalias=".create_user" )
 column=(type=datetime update=yes updatewhereclause=yes name=mod_date dbname="verif_info.mod_date" dbalias=".mod_date" )
 column=(type=char(10) update=yes updatewhereclause=yes name=mod_user dbname="verif_info.mod_user" dbalias=".mod_user" )
 retrieve="  SELECT verif_info.prac_id,   
         verif_info.rec_id,   
         verif_info.seq_no,   
         verif_info.facility_id,   
         verif_info.print_flag,   
         facility_ver_rules.ver_letter_resend_days,   
         verif_info.last_sent,   
         verif_info.last_sent + facility_ver_rules.ver_letter_resend_days,
         verif_info.create_date,  
         verif_info.create_user,   
         verif_info.mod_date,   
         verif_info.mod_user    
    FROM facility_ver_rules,   
         verif_info  
   WHERE ( verif_info.screen_id = facility_ver_rules.screen_id ) and  
         ( verif_info.facility_id = facility_ver_rules.facility_id ) and  
         ( verif_info.response_code IS NULL ) AND  
         ( verif_info.print_flag = 0 ) AND  
         (isnull(verif_info.number_sent,0) < facility_ver_rules.ver_letter_max_sends) AND  
         ( verif_info.last_sent + facility_ver_rules.ver_letter_resend_days < getdate()) and
		( verif_info.verification_method <> 17) and 
			verif_info.wf_id is null  
union all
  SELECT verif_info.prac_id,   
         verif_info.rec_id,   
         verif_info.seq_no,   
         verif_info.facility_id,   
         verif_info.print_flag,   
         wf_action_status.day_interval,   
         verif_info.last_sent,   
         verif_info.last_sent + wf_action_status.day_interval,
         verif_info.create_date,  
         verif_info.create_user,   
         verif_info.mod_date,   
         verif_info.mod_user    
    FROM wf_action_status,   
         verif_info  
   WHERE ( verif_info.wf_id = wf_action_status.wf_id ) and  
			( verif_info.wf_action_type_id = wf_action_status.step_id ) and  
         ( verif_info.response_code = wf_action_status.action_status_id ) and  
         ( verif_info.print_flag <= 0 ) AND
         ( wf_action_status.process_flag = 1 ) AND
         ( wf_action_status.letter_id is not null ) AND
         (isnull(verif_info.number_sent,0) < wf_action_status.resend_times) AND  
         ( verif_info.last_sent + wf_action_status.day_interval < getdate() )   
    
" update="verif_info" updatewhere=0 updatekeyinplace=yes )

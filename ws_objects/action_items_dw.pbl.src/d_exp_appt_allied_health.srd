 column=(type=decimal(0) updatewhereclause=yes name=prac_id dbname="pd_affil_stat.prac_id" dbalias=".prac_id" )
 column=(type=long updatewhereclause=yes name=parent_facility_id dbname="pd_affil_stat.parent_facility_id" dbalias=".parent_facility_id" )
 column=(type=datetime updatewhereclause=yes name=apptmnt_end_date dbname="pd_affil_stat.apptmnt_end_date" dbalias=".apptmnt_end_date" )
 column=(type=long updatewhereclause=yes name=verifying_facility dbname="pd_affil_stat.verifying_facility" dbalias=".verifying_facility" )
 column=(type=datetime updatewhereclause=yes name=pd_affil_stat_apptmnt_end_date dbname="pd_affil_stat.apptmnt_end_date" dbalias=".apptmnt_end_date" )
 retrieve="  SELECT DISTINCT pd_affil_stat.rec_id,   
         pd_affil_stat.prac_id,   
         pd_affil_stat.parent_facility_id,   
         pd_affil_stat.apptmnt_end_date,   
         pd_affil_stat.verifying_facility,
	pd_affil_stat.apptmnt_end_date  
    FROM pd_affil_stat,   
         facility,   
         code_lookup,   
         pd_basic  
   WHERE ( facility.facility_id = pd_affil_stat.parent_facility_id ) and  
         ( pd_affil_stat.prac_id = pd_basic.prac_id ) and  
         ( pd_basic.prac_category = code_lookup.lookup_code ) and  
         (  pd_affil_stat.active_status = 1 ) AND
  			( upper(code_lookup.custom_6) = 'AH') and
         ( pd_affil_stat.apptmnt_end_date <= getdate() + facility.reappointment_query_days ) AND 
			( facility.reappointment_query_days > 0 ) and 
         ( pd_affil_stat.prac_id not in ( SELECT DISTINCT verif_info.prac_id FROM verif_info WHERE (verif_info.reference_value = 'Expiring Appointment Letter' or verif_info.reference_value = 'Expiring Allied Health Letter' or verif_info.reference_value = 'Expiring Provisional Letter') and active_status = 1 and verif_info.facility_id = pd_affil_stat.parent_facility_id) )   
            
" )

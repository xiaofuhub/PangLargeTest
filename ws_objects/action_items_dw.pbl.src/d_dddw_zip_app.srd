 retrieve="  SELECT distinct net_dev_action_items.app_id,   
		   app_hdr.application_name
    FROM net_dev_action_items 
             LEFT OUTER JOIN app_hdr ON net_dev_action_items.app_id = app_hdr.app_id,	
             net_dev_action_images,
			code_lookup				  
   WHERE  (net_dev_action_items.prac_id = :al_prac_id)  and  
			  (net_dev_action_items.app_id = net_dev_action_images.app_id)  and  
                (datalength(net_dev_action_images.image_data) > 0) and
       		  (net_dev_action_items.facility_id = :al_facility_id)  and
               (net_dev_action_items.action_type = code_lookup.lookup_code) and
               (code_lookup.lookup_name = 'Action Type' and code_lookup.code = 'App Out To MD')
              
" arguments=(("al_prac_id", number),("al_facility_id", number)) )

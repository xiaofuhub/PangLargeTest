 retrieve="  SELECT meetings.meeting_id,   
         meetings.facility,   
         meetings.meeting_type,   
         meetings.start_date,   
         meetings.start_time,   
         meetings.end_date,   
         meetings.end_time,   
         meetings.subject,   
         meetings.location,   
         meetings.committee,   
         meetings.comments,   
         meetings.cme_credits,   
         meetings.cme_category,   
         meetings.cme_presenter,    
         meetings.cme_affiliation,   
         meetings.department,   
         meetings.section,   
         meetings.start_at,   
         meetings.end_at ,
         meetings.meet_profile ,
         meetings.support_rcvd ,
         meetings.support_amount ,
         meetings.purpose_for ,   
         meetings.committee_lookup ,
         datalen = datalength(document_storage.Doc_blob)     
    FROM {oj meetings left outer join document_storage on Record_id = meeting_id and meetings.facility = document_storage.facility_id and Function_type  = 'meetings'} ,    
         facility,   
         security_user_facilities  
   WHERE ( facility.facility_id = security_user_facilities.facility_id ) and  
         ( meetings.facility = facility.facility_id ) and  
         ( ( meetings.start_date = :date_1 ) AND  
         ( security_user_facilities.access_rights = 1 ) AND  
         ( security_user_facilities.user_id = :userid ) )    
" update="meetings" updatewhere=0 updatekeyinplace=no arguments=(("date_1", datetime),("userid", string)) )

 retrieve="  SELECT code_lookup_a.code,   
         meetings.start_date,   
         meetings.end_date,   
         meetings.subject,   
         code_lookup_b.code,   
         meetings.location,   
         meetings.committee,   
         meetings.comments,   
         meetings.cme_credits,   
         meetings.cme_category,   
         meetings.cme_presenter,   
         meetings.cme_affiliation,   
         meetings.facility,   
         attendence.attendence_status,   
         meetings.meeting_id,   
         attendence.meeting_id,   
         attendence.prac_id,   
         pd_basic.first_name,   
         pd_basic.middle_name,
         starttime =  meetings.start_at,   
         endtime =  meetings.end_at,   
         pd_basic.last_name,   
         v_affil_staff_cat_active.staff_cat  
    FROM v_affil_staff_cat_active  RIGHT OUTER JOIN attendence ON v_affil_staff_cat_active.prac_id = attendence.prac_id and v_affil_staff_cat_active.facility_id=(select top 1 facility from meetings where meeting_id=:meeting_id),
		meetings left outer join code_lookup code_lookup_b	on meetings.location = code_lookup_b.lookup_code,
         code_lookup code_lookup_a,   
         pd_basic
   WHERE ( attendence.meeting_id = meetings.meeting_id ) and  
         ( meetings.meeting_type = code_lookup_a.lookup_code ) and 
         ( attendence.prac_id = pd_basic.prac_id ) and  
        	( attendence.prac_id  > 0)  and
         ( ( attendence.meeting_id = :meeting_id ) ) 


union

  SELECT code_lookup_a.code,   
         meetings.start_date,   
         meetings.end_date,   
         meetings.subject,   
         code_lookup_b.code,   
         meetings.location,   
         meetings.committee,   
         meetings.comments,   
         meetings.cme_credits,   
         meetings.cme_category,   
         meetings.cme_presenter,   
         meetings.cme_affiliation,   
         meetings.facility,   
         attendence.attendence_status,   
         meetings.meeting_id,   
         attendence.meeting_id,   
         attendence.prac_id,   
         pd_basic.first_name,   
         pd_basic.middle_name, 
         starttime = meetings.start_at,   
         endtime = meetings.end_at,   
         pd_basic.last_name,   
         pd_basic.cust_7 
    FROM meetings left outer join code_lookup code_lookup_b on meetings.location = code_lookup_b.lookup_code ,
			attendence, 
         code_lookup code_lookup_a,   
         pd_basic
   WHERE ( attendence.meeting_id = meetings.meeting_id ) and  

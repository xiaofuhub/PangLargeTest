 retrieve="  SELECT ctx_am_action_item.action_type,   
			case when ctx_am_action_item.wf_id is null then isnull(code_lookup.custom_6,'') else isnull(sys_letters.document_name,'') end as document_name,
			/*case when ctx_am_action_item.wf_id is null then isnull(code_lookup.description,'') else isnull(wf_action_types.action_desc,'') end as action_description,*/         
			code_lookup.description as action_description,
			sys_letters.letter_name,
			sys_letters.use_export,
			sys_letters.export_id,   
			ctx_am_document.ctx_id,
         ctx_am_action_item.doc_id,   
         code_lookup.lookup_code,   
         ctx_am_action_item.seq_id,   
         ctx_am_action_item.first_attempt,   
         ctx_am_action_item.last_attempt,   
         ctx_am_action_item.attempt_times,   
         ctx_am_action_item.print_flag,   
         ctx_am_action_item.wf_id,   
         ctx_am_action_item.letter_id,   
         ctx_am_action_item.wf_action_type_id,   
         ctx_am_action_item.wf_step,   
         ctx_am_action_item.action_status  
    FROM code_lookup,   
         ctx_am_action_item left outer join sys_letters on ctx_am_action_item.letter_id = sys_letters.letter_id
                                      left outer join wf_action_types on ctx_am_action_item.wf_id = wf_action_types.wf_id and ctx_am_action_item.wf_action_type_id = wf_action_types.step_id,
			ctx_am_document
   WHERE ( ctx_am_action_item.action_type = code_lookup.lookup_code ) and  
			( ctx_am_action_item.seq_id in ( :al_action_item_id ) ) AND
         ((ctx_am_action_item.wf_id is null and code_lookup.custom_6 is not null ) or (ctx_am_action_item.wf_id > 0 and ctx_am_action_item.letter_id > 0 and ctx_am_action_item.print_flag > 0)) and
			ctx_am_document.doc_id = ctx_am_action_item.doc_id
	order by document_name

" update="ctx_am_action_item" updatewhere=0 updatekeyinplace=no arguments=(("al_action_item_id", numberlist)) )

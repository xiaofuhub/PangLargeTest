 retrieve="SELECT Distinct group_multi_loc_gp_name = group_multi_loc.gp_name,
	 group_multi_loc_rec_id = group_multi_loc.rec_id,
	 group_practice_street = IsNull(group_practice.street, '**No Linked Locations**') ,
	 group_practice_rec_id = IsNull(group_practice.rec_id, 0) , 
	 group_loc_link_group_id = group_loc_link.group_id, 
	 group_practice.alias_name,  
	 pd_address_prac_id = IsNull(pd_address.prac_id, 0),
 	 v_full_name_full_name = IsNull(NullIf(rtrim(v_full_name.full_name), ','), '**No Associated Practitioners**') ,
	  pd_address.active_status,
	gp_active = group_practice.active_status,
	is_active = (select count(active_status) from pd_affil_stat where prac_id = pd_address.prac_id and active_status in (1,4))
FROM group_multi_loc LEFT OUTER JOIN 
	(group_loc_link JOIN 
		(group_practice LEFT OUTER JOIN 
			(pd_address JOIN v_full_name ON pd_address.prac_id = v_full_name.prac_id
			) ON group_practice.rec_id = pd_address.gp_id
		) ON group_loc_link.loc_id = group_practice.rec_id
	 ) ON group_multi_loc.rec_id = group_loc_link.group_id
WHERE group_multi_loc.rec_id = :ai_group_id
" arguments=(("ai_group_id", number))  sort="group_multi_loc_gp_name A group_multi_loc_rec_id A group_practice_street A group_practice_rec_id A v_full_name_full_name A pd_address_prac_id A " )

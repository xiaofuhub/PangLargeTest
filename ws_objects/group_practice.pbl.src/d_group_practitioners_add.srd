 retrieve="  SELECT 
		pd_basic.last_name,  
		pd_basic.first_name,
		pd_basic.middle_name,
		pd_basic.title,
         pd_address.street,   
         pd_address.accepting_new_patients,   
         pd_address.active_status,   
         pd_address.street_2,   
          v_primary_specialty.specialty_id,   
          v_primary_specialty.specialty_name,   
         pd_address.gp_id,   
         pd_address.rec_id,
			pd_address.prac_id  ,
	selected = 0
    FROM pd_basic LEFT OUTER JOIN v_primary_specialty ON pd_basic.prac_id =  v_primary_specialty.prac_id,   
         group_practice,   
         pd_address  
   WHERE ( pd_address.prac_id = pd_basic.prac_id ) and  
         ( group_practice.rec_id = pd_address.gp_id ) and  
          ( pd_address.rec_id in (:ai_pracs) )     
" arguments=(("ai_pracs", numberlist)) )

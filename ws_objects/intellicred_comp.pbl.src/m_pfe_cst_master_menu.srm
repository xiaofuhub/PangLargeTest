	window lw_temp
	datawindow ldw_temp
	commandbutton lcb_temp
	string ls_info



	if IsValid(w_mdi.GetActiveSheet()) then
		lw_temp = w_mdi.GetActiveSheet()
	else
		lw_temp = w_mdi
	end if

	ls_info = "Current window = " + lw_temp.ClassName() + " (" + lw_temp.Title + ")" + "~n"


	Choose case TypeOf(GetFocus())
		case Datawindow!
			ldw_temp = GetFocus()
			ls_info = ls_info + "Current object = " + ldw_temp.ClassName() + " (" + ldw_temp.DataObject + ")"
		case CommandButton!
			lcb_temp = GetFocus()
			ls_info = ls_info + "Current object = " + lcb_temp.ClassName() + " (" + lcb_temp.Text + ")"

	end choose

	MessageBox("Object Information", ls_info)
end event


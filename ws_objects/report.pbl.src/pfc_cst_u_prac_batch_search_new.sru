event clicked;Long r
//---------Begin Add  by (Appeon)Stephen 2013-06-20 for IntelliCred Times Out for LARGE OIG Scans--------
datastore lds_prac_temp
long ll_pracs_temp[], ll_null[]
long ll_i, li_next, li_temp, ll_k
//---------End Modfiied ------------------------------------------------------

IF is_openedfor = "E" THEN
	w_batch_prac_select_new.ii_close = 1
	CloseWithReturn( w_batch_prac_select_new,is_sql )
	
ELSEIF is_openedfor = "S" THEN //Start Code Change ----10.02.2008 #V85 maha - added for search
	CloseWithReturn( w_batch_prac_select_new,il_batch)
ELSE //A,P,R,B,G
	//Start Code Change ----08.12.2011 #V11 maha  - moved from of_run
	dw_prac_list.settransobject(sqlca)
	//Start Code Change ----08.12.2011
	
	//---------Begin Modified by (Appeon)Stephen 2013-06-20 for IntelliCred Times Out for LARGE OIG Scans--------
	/*dw_prac_list.retrieve(il_pracs[])
	*/
	if upperbound(il_pracs) > 6000 then
		ll_i = 1
		lds_prac_temp = create datastore
		lds_prac_temp.dataobject = dw_prac_list.dataobject
		lds_prac_temp.settransobject(sqlca)
		do while ll_i <= upperbound(il_pracs)
			ll_pracs_temp = ll_null
			li_next = ll_i + 5999
			if li_next >= upperbound(il_pracs) then li_next = upperbound(il_pracs) 
			li_temp = 1
			for ll_k = ll_i to li_next
				ll_pracs_temp[li_temp] = il_pracs[ll_k]
				li_temp +=1
			next
			ll_i = li_next +1
			//lds_prac_temp.retrieve(ll_pracs_temp[])
			lds_prac_temp.retrieve(ll_pracs_temp[], gs_user_id)  //Start Code Change ----05.30.2017 #V154 maha - facility security
			lds_prac_temp.rowscopy(1, lds_prac_temp.rowcount(),primary!, dw_prac_list,dw_prac_list.rowcount()+1,primary!)
		loop
		destroy lds_prac_temp		
	else 
		//dw_prac_list.retrieve(il_pracs[])   //Commented by (Appeon)Harry 10.29.2014 - Q on data window error
		//if upperbound(il_pracs) > 0 then dw_prac_list.retrieve(il_pracs[])  //(Appeon)Harry 10.29.2014 - Q on data window error
		if upperbound(il_pracs) > 0 then dw_prac_list.retrieve(il_pracs[] , gs_user_id)  //Start Code Change ----05.30.2017 #V154 maha - facility security
	end if
	//---------End Modfiied ------------------------------------------------------
		
	IF dw_prac_list.RowCount() < 1 THEN
		w_batch_prac_select_new.ii_close = 1
		CloseWithReturn( w_batch_prac_select_new ,"Cancel")
	ELSE
		//for applications get app and facility
		IF is_openedfor = "A" THEN
			IF il_app_id < 1 THEN //maha 030805
				MessageBox("Need Application","Please select an Application.")
				RETURN
			END IF
			IF il_facility_id < 1  THEN
				OpenWithParm(w_select_facility,"A") //maha 051205
				//w_select_facility.st_1.visible = false 
				il_facility_id = Message.DoubleParm
				IF il_facility_id < 1  THEN
					MessageBox("Need Facility","Must have a facility selected to return with the practitioner batch.")
					RETURN
				END IF
			END IF
		END IF

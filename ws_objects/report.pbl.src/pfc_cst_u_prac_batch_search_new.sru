event clicked;long ll_pracs[]
integer i
integer r
integer pc
String ls_presentation_str
String ls_dwsyntax_str
string errors
integer w
long ll_ret_cnt
long org_cnt
n_ds lds_prac_list
integer li_onetable = 0
integer res
long nr

// mskinner 20 dec 2005 -- begin
if dw_batch_detail.rowcount() < 1 then return 0 
// mskinner 20 dec 2005 -- end 

if il_batch < 1 then return
//messagebox("In Process","Unable to save batch Items")
//return -1
is_sql = dw_batch_detail.getitemstring(1,"create_sql")
if isnull(is_sql) or is_sql = "" then
	messagebox("Rebuild batch list ","No saved query to recreate Batch Items; Rerun select.")
	return -1
end if

res = messagebox("Rebuild batch list ","This function will recreate the list from the original selected criteria,~rand will override any modifications that have been made to the list.~r~r                 Do you wish to continue?",Question!,yesno!,1)
if res = 2 then return
		
ls_presentation_str = "style(type=grid)"		

ls_dwsyntax_str = SQLCA.SyntaxFromSQL(is_sql, ls_presentation_str, ERRORS)
IF LenA(ERRORS) > 0 THEN
	MessageBox("Caution on export field " + is_sql, &
	"SyntaxFromSQL caused these errors: " + ERRORS + "~r~r" + is_sql)
//	MessageBox("ls_select_table",ls_select_table)
	
	RETURN -1
END IF

lds_prac_list = CREATE n_ds
w = lds_prac_list.Create( ls_dwsyntax_str, ERRORS)
if w < 1 then
	messagebox("error on create",errors)
	return -1
end if

lds_prac_list.SetTransObject( SQLCA )
ll_ret_cnt = lds_prac_list.retrieve()
st_help_text.text = string(ll_ret_cnt) + " records"
		
debugbreak()		
	//create structure	
if ll_ret_cnt > 0 then
	for r = 1 to ll_ret_cnt
		//Start Code Change ----06.04.2013 #V14 maha - added pd_basic trap for saved from search screen
//		if pos(is_sql, "pd_basic.prac_id",1) > 0 then
//			ll_pracs[r] =  Long(lds_prac_list.of_getitem( 'pd_basic_prac_id', r))
//		else
//			ll_pracs[r] =  Long(lds_prac_list.of_getitem( 'v_full_name_prac_id', r))
//		END IF
		//---------Begin Modified by (Appeon)Stephen 08.01.2014 for Scan OIG Function--------
		/*
		if pos(is_sql, "v_full_name.prac_id",1) > 0 then
			ll_pracs[r] =  Long(lds_prac_list.of_getitem( 'v_full_name_prac_id', r))
		else
			ll_pracs[r] =  Long(lds_prac_list.of_getitem( 'pd_basic_prac_id', r))
		END IF
		*/
		if pos(is_sql, "v_full_name.prac_id",1) > 0 then
			ll_pracs[r] =  lds_prac_list.getitemnumber(r, 'v_full_name_prac_id')
		else
			ll_pracs[r] =  lds_prac_list.getitemnumber(r,  'pd_basic_prac_id')
		END IF
		//---------End Modfiied ------------------------------------------------------

	next

	dw_prac_list.settransobject(sqlca)
	pc = dw_prac_list.retrieve(ll_pracs[], gs_user_id)   //Start Code Change ----05.31.2017 #V154 maha - added user arg
end if		

if ll_ret_cnt <> upperbound(ll_pracs[]) then messagebox("","Get prac id list failed")  //Start Code Change ----11.07.2014 #V14.2 maha
	
org_cnt = dw_batch_items.rowcount()
for r = org_cnt to 1 step -1
	dw_batch_items.deleterow(r)
next
for r = 1 to pc
	nr = dw_batch_items.insertrow(r)	
	dw_batch_items.setitem(nr,"batch_id",il_batch)
	dw_batch_items.setitem(nr,"item_id",nr)
	dw_batch_items.setitem(nr,"prac_id",dw_prac_list.getitemnumber(r,"prac_id"))
//	ist_search.ls_name[r] = lds_prac_list.getitemstring(r,"v_full_name_full_name")
next

res = dw_batch_items.update()
if res < 1 then
	messagebox("Save Error","Unable to save batch Items")
	return -1
else
	messagebox("Batch updated","Batch contained " + string(org_cnt) + " names; now contains " + string(pc) + " names.")
end if
commit using sqlca;

of_log_batch_change(org_cnt, pc, il_batch, gs_user_id)

destroy lds_prac_list

return 1






end event


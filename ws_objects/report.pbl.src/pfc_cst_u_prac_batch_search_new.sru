string ls_obj
datetime ldt_from
datetime ldt_to

if is_openedfor = "A" and il_app_id < 1 then
	messagebox("Start Error","Please select an application")
	return
else
	//dw_app.enabled = false
end if

dw_wizard.reset()
//create list of wizards
if cbx_facility.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_org_facility")
	if cbx_facility.enabled = false then
		//set facility
	end if
	lb_facility = true  //Start Code Change ----09.26.2014 #V14.2 maha
end if

if cbx_status.checked = true then
//	if lb_facility = false then messagebox("Start error","One or more selections")
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_appmnt_status")
end if

//Start Code Change ----06.04.2013 #V14 maha
if cbx_appt_type.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_appmnt_type")
end if
//End Code Change ----06.04.2013

//Start Code Change ----06.26.2013 #V14 maha
if cbx_appt_start.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_appt_date_range") 
	ib_appt_start = true
end if
//End Code Change ----06.26.2013

if cbx_exp.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_appt_date_range")
	ib_appt_end = true
end if

if cbx_provis.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_prov_date_range")
end if

if cbx_dept.checked = true then
	r = dw_wizard.insertrow(0)
	//check to see if department security is on
	Integer li_dept_secure_setting

//	SELECT set_37
//	INTO: ii_dept_security_on
//	FROM icred_settings;
//	IF SQLCA.SQLCODE = -1 THEN
//		MessageBox("SQL Error", SQLCA.SQLERRTEXT )
//		RETURN 1
//	END IF
	ii_dept_security_on = gi_dept_security

	IF ii_dept_security_on = 1 THEN
		dw_wizard.setitem(r,"wizard_dw","d_crit_department_with_security")
	ELSE
		dw_wizard.setitem(r,"wizard_dw","d_crit_department")
	END IF
end if

if cbx_div.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_division")
end if

if cbx_sect.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_section")
end if

if cbx_affil_stat.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_affil_status")
end if

//--------------------------- APPEON BEGIN ---------------------------
//$<add> 2006-09-20 By: Rodger Wu (Inova)
//$<reason> Add a checkbox called Assigned to for Invoa Customizations.
if cbx_priority_user.checked then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_user_multi")
end if
//---------------------------- APPEON END ----------------------------

if cbx_staff_cat.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_staff_cat")
end if

if cbx_committee.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_committee")
end if

if cbx_specialty.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_specialties")
end if

if cbx_prac_cat.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_prac_category")
end if

if cbx_prof_suf.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_prof_suffix")
end if

if cbx_lname.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_last_name")
end if

if cbx_contracts.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_contracts")
end if

if cbx_state.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_state")
end if

if cbx_city.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_city")
end if

if cbx_county.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_county")
end if

if cbx_addr_type.checked = true then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_address_type")
end if

//Start Code Change ----01.24.2012 #V12 maha
if cbx_priv_req.checked then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_priv_status")
end if
//End Code Change ----01.24.2012

//Start Code Change ----07.10.2014 #V14.2 maha
if cbx_network.checked then
	r = dw_wizard.insertrow(0)
	dw_wizard.setitem(r,"wizard_dw","d_crit_net_dev_type_lookup_code")
end if
//End Code Change ----07.10.2014

rc = dw_wizard.rowcount()
il_wizard_step_cnt = rc
st_1.text = "1 of " + string(rc)

if rc < 1 then return

cb_next.enabled = true


of_reset()


end event


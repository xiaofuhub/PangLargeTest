		//---------------APPEON BEGIN---------------
		//<$>added long.zhang 04.05.2012
		//<$Reason> should display correct active Status
		IF is_openedfor = "G" THEN
				if isvalid(w_batch_add_address) then w_batch_add_address.is_appmnt_status = is_appmnt_status
		END IF
		//----------------APPEON END------------------
		//Start Code Change ----01.27.2015 #V14.2 maha - allow sub-select
		if cbx_sub.checked then
			openwithparm(w_batch_sub_select,ist_search )
			if message.stringparm = "Cancel" then
				
			else
				ist_search = message.powerobjectparm
			end if
		end if
		//End Code Change ----01.27.2015
		
		CloseWithReturn( w_batch_prac_select_new,ist_search )
	END IF
END IF


$PBExportHeader$w_office_display.srw
forward
global type w_office_display from w_popup
end type
type ole_office from u_office_ocx within w_office_display
end type
end forward

global type w_office_display from w_popup
integer width = 3264
integer height = 2080
string title = "Document Preview"
windowstate windowstate = maximized!
ole_office ole_office
end type
global w_office_display w_office_display

type variables
str_office_ocx istr_parms
end variables

on w_office_display.create
int iCurrent
call super::create
this.ole_office=create ole_office
iCurrent=UpperBound(this.Control)
this.Control[iCurrent+1]=this.ole_office
end on

on w_office_display.destroy
call super::destroy
destroy(this.ole_office)
end on

event open;call super::open;//Alfee 07.14.2009

IF NOT IsValid(Message.powerobjectparm) THEN RETURN
		
istr_parms = Message.powerobjectparm

//Open the speicifed document 
IF FileExists(istr_parms.file_name) THEN
	ole_office.of_open(istr_parms.file_name, istr_parms.read_only)
ELSE
	ole_office.of_open(istr_parms.file_data, istr_parms.file_ext, istr_parms.read_only)
END IF

//Display file name in the title
IF NOT IsNull(istr_parms.file_name_ori) THEN
	This.title += ' - ' + istr_parms.file_name_ori
END IF

	

		
	
end event

event pfc_preopen;call super::pfc_preopen;string ls_scale
long 	 ll_i

this.of_SetResize(TRUE)

this.inv_resize.of_SetOrigSize (this.workspacewidth( ) ,  this.workspaceheight( ) )
ls_scale = this.inv_resize.scale

FOR LL_I =  1 TO UPPERBOUND(this.CONTrol)
   this.inv_resize.of_Register (this.CONTrol[LL_I]  , ls_scale	 )
NEXT
end event

type ole_office from u_office_ocx within w_office_display
integer width = 3232
integer height = 1984
integer taborder = 20
string binarykey = "w_office_display.win"
end type



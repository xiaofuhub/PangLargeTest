 retrieve="  SELECT sys_letters.letter_id,   
         sys_letters.response_required,   
         sys_letters.maximum_sends,   
         sys_letters.resend_days,   
         sys_letters.use_export,   
         sys_letters.export_id,   
         sys_letters.net_dev_letter,   
         sys_letter_module.letter_module,   
         sys_letter_module.letter_destination,   
         sys_letter_module.letter_id,   
         sys_letter_module.user_comment,   
         sys_letters.letter_name,   
         sys_letters.letter_description,   
         sys_letters.document_name,
			sys_letters.case_review_letter
    FROM sys_letter_module RIGHT OUTER JOIN sys_letters ON sys_letter_module.letter_id = sys_letters.letter_id 
   WHERE ( sys_letters.letter_id = :letter_id )    
" update="sys_letters" updatewhere=1 updatekeyinplace=no arguments=(("letter_id", number)) )

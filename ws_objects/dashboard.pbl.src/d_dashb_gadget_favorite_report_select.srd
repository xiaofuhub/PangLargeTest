SELECT Distinct
	Selected = (
	Case When
		Exists(Select 1 from dashboard_gadgets_favorite where dashboard_gadgets_favorite.data_id = sys_reports.report_id and dashboard_gadgets_favorite.user_id = :as_user_id and dashboard_gadgets_favorite.favorite_type = 'REPORT')
	Then 1
	Else 0
	End),

	sys_reports.report_id,   
	sys_reports.report_name,
	sys_reports.category, 
	'REPORT' report_type
From 
	sys_reports

Union

Select Distinct
	selected = (
	Case When
		Exists(Select 1 from dashboard_gadgets_favorite where dashboard_gadgets_favorite.data_id = conv_view.view_id and dashboard_gadgets_favorite.user_id = :as_user_id and dashboard_gadgets_favorite.favorite_type = 'INTELLIREP')
	Then 1
	Else 0
	End),

	conv_view.view_id,
	conv_view.view_name,
	conv_view.category,
	'INTELLIREP' report_type	

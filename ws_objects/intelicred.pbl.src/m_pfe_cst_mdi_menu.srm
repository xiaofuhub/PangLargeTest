type m_runactionitemsquery from menu within m_system
end type

event clicked; //Reconstructed by (Appeon)Alfee 06.27.2013 - V141 ISG-CLX
Long ll_max_id
n_cst_rights lnv_right
n_cst_email_alarm lnv_email_alarm
n_cst_workflow_triggers	lnv_workflow
pfc_n_cst_ai_printletters lnv_exp_cred

//Run AI Query for IntelliCred/App
IF NOT gb_contract_version THEN
	lnv_exp_cred = CREATE pfc_n_cst_ai_printletters
	IF appeongetclienttype() = 'WEB' THEN StopIEpopupmsg()
	lnv_exp_cred.of_run_ai_query(0) 
	IF appeongetclienttype() = 'WEB' THEN ResumeIEpopupmsg()
END IF	

DESTROY lnv_exp_cred
IF NOT gb_contract_module THEN RETURN 

//For Contract ...
//Check Rights to the Temporary Directory 
lnv_right = Create n_cst_rights
If lnv_right.of_check_tempdir_right( ) < 0 then 
	Destroy lnv_right
	Return
End If
Destroy lnv_right

w_mdi.inv_statusbar.of_setrefreshrate( 0 ) //Workaround APB Bug
Sleep(1)	
gb_ShowEmailErrorMessage = False
lnv_workflow = Create n_cst_workflow_triggers
Select Max(id) into :ll_max_id From em_mail_audit Where user_id = :gs_user_id;
lnv_workflow.of_set_message( false) 

//Run Email Alarm
lnv_email_alarm.of_run_email_alarm( )  //lnvo_notification.of_start_notification_process( ) & lnv_workflow.of_reminder_mail_process()

//Trigger Work Flow (CTX)
lnv_workflow.of_trigger_by_onlogin( )
	
//Send checkin email.
n_cst_email_checkin lnv_email_checkin
lnv_email_checkin = Create n_cst_email_checkin
lnv_email_checkin.of_send_email()
If Isvalid(lnv_email_checkin) Then Destroy lnv_email_checkin

//Check CTX letters to be reprinted 
lnv_workflow.of_resend_letters( )
Destroy n_cst_workflow_triggers

//Refresh Action Items data and Doc Action Items data in Contract Folder
IF gb_contract_version THEN 
	If Isvalid(gw_contract) Then
		If w_contract.tab_contract_details.ib_select10 = True Then
			w_contract.tab_contract_details.tabpage_action_items.tab_1.tabpage_detail.of_retrieve()
		End If				
	End If
End If

f_show_email_result(ll_max_id)
gb_ShowEmailErrorMessage = True
w_mdi.inv_statusbar.of_setrefreshrate( 3 )


//IF gb_contract_module THEN 
//	// start the email
//	n_cst_notification  lnvo_notification
//	lnvo_notification =  create n_cst_notification
//	lnvo_notification.of_start_notification_process( )
//	if IsValid(lnvo_notification) then Destroy lnvo_notification
//
//	IF gb_contract_version THEN Return 
//END IF
//
//pfc_n_cst_ai_printletters lnv_exp_cred
//lnv_exp_cred = CREATE pfc_n_cst_ai_printletters
//IF appeongetclienttype() = 'WEB' THEN StopIEpopupmsg()
//lnv_exp_cred.of_run_ai_query(0) //maha 082903
//IF appeongetclienttype() = 'WEB' THEN ResumeIEpopupmsg()
//
//DESTROY lnv_exp_cred
	





end event


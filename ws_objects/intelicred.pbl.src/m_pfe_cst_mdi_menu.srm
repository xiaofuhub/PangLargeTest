event clicked;
//---------Begin Modified by (Appeon)Alfee 06.27.2013 for V141 ISG-CLX--------
//OpenSheetWithParm(w_email_sent_items, "Personal!", w_mdi, 4, Original!) // evan 06.19.2012

//Added By Alan on 2009-2-19
int	li_opentype,li_tabpage
str_email_parm lstr_email_parm
Long ll_ctx_id

If not Isvalid(gw_contract) Then
	li_opentype=0
else
	li_tabpage=gw_contract.tab_contract_details.selectedtab
	if li_tabpage=9 then
		li_opentype=2
	elseif li_tabpage=10 then	
		li_opentype=3
	else
		li_opentype=1
	end if
	If gw_contract.tab_contract_details.tabpage_search.dw_search_det1.getrow() > 0 Then
		ll_ctx_id = gw_contract.tab_contract_details.tabpage_search.dw_search_det1.getitemdecimal(gw_contract.tab_contract_details.tabpage_search.dw_search_det1.getrow(),"ctx_id")
	End If
	
end if	

//Added By Ken.Guo 2010-03-29.
lstr_email_parm.ai_folder_type = 2 //Inbox
lstr_email_parm.ai_from_type = li_opentype 

If isvalid(w_email_folder) Then
	w_email_folder.BringtoTop = True
	If w_email_folder.WindowState = Minimized! Then
		w_email_folder.WindowState = Normal!
	End If
	w_email_folder.SetFocus()
Else
	//---------Begin Added by (Appeon)Harry 09.10.2014 for V142 ISG-CLX--------
	boolean lb_active_ctx = false
	if isvalid (w_mdi.GetActiveSheet( )) and Upper(AppeonGetClienttype()) = 'WEB'  then 
		if w_mdi.GetActiveSheet( ).classname() = 'w_contract' then lb_active_ctx = true 
	end if 
	//---------End Added ------------------------------------------------------
	OpenSheetwithparm(w_email_folder,lstr_email_parm, w_mdi, 4, Original!) 	
	//---------Begin Added by (Appeon)Harry 09.10.2014 for V142 ISG-CLX--------
	if Upper(AppeonGetClienttype()) = 'WEB'  and lb_active_ctx then
		w_email_folder.ib_tdl_ctx = true 
	end if
	//---------End Added ------------------------------------------------------
End If
//---------End Modfiied ----------------------------------------------------------------

end event


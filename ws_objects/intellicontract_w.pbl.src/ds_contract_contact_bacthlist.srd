column=(type=long updatewhereclause=yes name=default_count dbname="ctx_contract_contacts.default_count"  )
 retrieve="select a.ctx_id,a.contact_id, a.ic_area, a.default_email, a.owner,
    a.association,   
         a.record_id,   
  b.rec ,b.default_count from  ctx_contract_contacts A inner join
( select  ctx_id, ic_area,count(contact_id ) rec,sum( ( case when ic_area = 'E' and default_email = 'Y' then 1 else 0 end ) ) default_count from ctx_contract_contacts group by ctx_id, ic_area  ) B
on A.ctx_id = B.ctx_id and A.ic_area = B.ic_area
   WHERE a.ctx_id in ( :al_ctx_id )    
"

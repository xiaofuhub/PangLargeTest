         ~"verif_info~".~"mod_user~" ,
	  ~"verif_info~".~"data_status~"
    FROM {oj ~"verif_info~" LEFT OUTER JOIN ~"address_lookup~" ON ~"verif_info~".~"address_lookup_code~" = ~"address_lookup~".~"lookup_code~"},   
         ~"data_view_screen~",   

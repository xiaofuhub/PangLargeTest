 retrieve="  SELECT pd_attest_questions.rec_id,   
         pd_attest_questions.prac_id,   
         pd_attest_questions.question_id,   
         pd_attest_questions.answer_code,   
         pd_attest_questions.active_status,   
         pd_attest_questions.appt_stat_id,   
         pd_attest_questions.facility_id,   
         pd_attest_questions.explain_yes  
    FROM pd_attest_questions  
   WHERE ( pd_attest_questions.prac_id = :ai_prac_id ) AND  
         ( pd_attest_questions.facility_id = :ai_facility_id )  AND
		EXISTS( select 1 from pd_affil_stat af 
			where af.rec_id = pd_attest_questions.appt_stat_id 
			and  (af.active_status = 1 or af.active_status = 3 or af.active_status = 4) ) 
  
" update="pd_attest_questions" updatewhere=0 updatekeyinplace=yes arguments=(("ai_prac_id", number),("ai_facility_id", number)) )

 retrieve="  SELECT distinct pd_images.active_status,   
         image_type.image_type_description,   
         pd_images.reference_name,   
         pd_images.num_pages,   
         notes = substring(pd_images.notes,   1, 8000),
         pd_images.date_scanned,   
         pd_images.user_name,   
         pd_images.date_recieved,   
         pd_images.tif_start_page,   
         pd_images.prac_id,   
         pd_images.facility_id,   
         pd_images.seq_no,   
         pd_images.screen_id,   
         image_type.link_to_screen,   
         pd_images.image_type_id,   
         selected = 0,   
         datalen = datalength(image_data ),   
         pd_images.rec_id,   
         image_type.image_order,   
         pd_images.image_pk,   
         pd_images.doc_id,   
         pd_images.de_queue,   
         view_rights = (case when security_user_images.view_rights  >0 then security_user_images.view_rights else 0 end),
		pd_images.image_ext
  FROM pd_images LEFT OUTER JOIN image_type ON pd_images.image_type_id = image_type.image_type_id 
               LEFT OUTER JOIN security_user_images ON image_type.image_type_id = security_user_images.image_type  and  security_user_images.user_id = :user_id 
			LEFT OUTER JOIN security_user_facilities ON pd_images.facility_id = security_user_facilities.facility_id  AND security_user_facilities.user_id =:user_id 
   WHERE (CASE WHEN pd_images.facility_id = 0 THEN 1 ELSE security_user_facilities.access_rights END = 1 )
      and	( pd_images.prac_id = :prac_id ) AND  
         (pd_images.de_queue = 1 OR  pd_images.de_queue is null)
" update="pd_images" updatewhere=0 updatekeyinplace=no retrieve.asneeded=yes arguments=(("prac_id", number),("user_id", string))  sort="image_type_image_order A image_type_description A reference_name A " )

         wf_action_status.send_count,   
         wf_action_status.mail_message,   
         wf_action_status.to_message_user,   
         wf_action_status.cc_message_user,   
         wf_action_status.letter_id,   
         wf_action_status.return_days,   
         wf_action_status.resend_times,   
         wf_action_status.day_interval,   
         wf_action_status.popup_flag,   
         wf_action_status.include_note_flag,   
         wf_action_status.process_flag,   
         wf_action_status.set_step,   
         wf_action_status.set_status,   
         wf_action_status.set_wf_id,   
         wf_action_status.prompt_flag,   
         wf_action_status.prompt_message,
			advanced = (select count(1) from  wf_advanced_update where wf_advanced_update.wf_id = :ai_wf_id  and wf_advanced_update.wf_step_id = :ai_step_id And wf_advanced_update.wf_status_id = wf_action_status.status_id )  ,
			wf_action_status.wf_esign
    FROM wf_action_status  
   WHERE ( wf_action_status.wf_id = :ai_wf_id ) AND  
         ( wf_action_status.step_id = :ai_step_id )    

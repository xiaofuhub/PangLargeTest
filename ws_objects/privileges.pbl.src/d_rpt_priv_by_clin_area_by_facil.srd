 retrieve="  SELECT facility.facility_name,   
         priv_clinical_area.clinical_area_code,   
         priv_clinical_area.clinical_area_description,   
         priv_core.priv_core_description,   
         priv_core_procd.core_procd_description,   
         priv_core_procd.sort_order,   
         priv_core_procd.procd_name,   
         priv_core.priv_core_id,   
         priv_core.sort_order  
    FROM facility LEFT OUTER JOIN
          ( priv_clinical_area  LEFT OUTER JOIN 
             ( priv_core LEFT OUTER JOIN priv_core_procd 
                ON priv_core_procd.priv_core_id = priv_core.priv_core_id)   
                 ON priv_core.clinical_area_id = priv_clinical_area.clinical_area_id)
       ON facility.facility_id = priv_clinical_area.facility_id
   WHERE ( facility.facility_id = :ai_facility ) AND  
         ( priv_clinical_area.clinical_area_id = :ai_clin_area )   
ORDER BY facility.facility_name ASC,   
         priv_clinical_area.clinical_area_code ASC,   
         priv_core.priv_core_description ASC,   
         priv_core_procd.sort_order ASC   
" arguments=(("ai_facility", number),("ai_clin_area", number))  sort="sort_order A priv_core_id A priv_core_procd_sort_order A " )

 column=(type=long updatewhereclause=yes name=priv_core_procd_active_status dbname="priv_core_procd.active_status" )
 retrieve="  SELECT priv_clinical_area.clinical_area_description,   
         priv_core.priv_core_description,   
         priv_core_procd.procd_name,     
         priv_clinical_area.clinical_area_id,   
         priv_core.priv_core_id,   
         priv_core_procd.procd_id,
         priv_core_procd.active_status
    FROM priv_clinical_area left outer join priv_core on (priv_clinical_area.clinical_area_id = priv_core.clinical_area_id)
         left outer join priv_core_procd on (priv_core.priv_core_id = priv_core_procd.priv_core_id)
   WHERE (priv_clinical_area.facility_id = :al_template_id)
	ORDER BY
			priv_clinical_area.clinical_area_description + cast(priv_clinical_area.clinical_area_id as varchar) ASC,
			priv_core.sort_order ASC,
			priv_core.priv_core_description ASC,
			priv_core_procd.sort_order ASC,
			priv_core_procd.procd_name ASC" arguments=(("al_template_id", number)) )

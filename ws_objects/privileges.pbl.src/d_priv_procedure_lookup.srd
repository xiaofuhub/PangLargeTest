 retrieve="  SELECT priv_procd_lookup.clinical_area_id,   
         code_lookup_a.description,   
         priv_procd_lookup.procd_name,   
         priv_procd_lookup.procd_code,   
         priv_procd_lookup.procedure_description,   
         priv_procd_lookup.procedure_pk,   
         priv_procd_lookup.special_requirement,   
         priv_procd_lookup.num_req_for_reapp,   
         priv_procd_lookup.section_id,   
         priv_procd_lookup.clinical_area_code,   
         priv_procd_lookup.dept_id,   
         priv_procd_lookup.active_status,   
         priv_procd_lookup.facility_specific,   
         priv_procd_lookup.specialty,   
         priv_procd_lookup.cross_area,   
         priv_procd_lookup.isg_code,   
         priv_procd_lookup.fppe_req,   
         code_lookup_b.description,   
         code_lookup_c.description ,
		selected = 0 
    FROM {oj priv_procd_lookup LEFT OUTER JOIN code_lookup code_lookup_c ON priv_procd_lookup.dept_id = code_lookup_c.lookup_code},   
         code_lookup code_lookup_a,   
         code_lookup code_lookup_b  
   WHERE ( priv_procd_lookup.clinical_area_id = code_lookup_a.lookup_code ) and  
         ( priv_procd_lookup.section_id = code_lookup_b.lookup_code ) 
" update="priv_procd_lookup" updatewhere=0 updatekeyinplace=no  sort="procd_name A procd_code A " )

 column=(type=char(218) updatewhereclause=yes name=v_full_name_full_name dbname="v_full_name.full_name" dbalias=".full_name" )
 retrieve="  SELECT pd_appl_tracking.app_id,   
         pd_appl_tracking.days_total,   
         pd_appl_tracking.assigned_user,   
         pd_appl_tracking.facility_id,   
         pd_appl_tracking.gp_id,   
         pd_appl_tracking.prac_id,   
         app_hdr.application_name,   
         pd_appl_tracking.step_1,  
		case   sys_appl_tracker.final_step 
				when 10 then pd_appl_tracking.step_10
				when 9 then pd_appl_tracking.step_9  
				when 8 then pd_appl_tracking.step_8   
				when 7 then pd_appl_tracking.step_7   
				when 6 then pd_appl_tracking.step_6   
				when 5 then pd_appl_tracking.step_5   
				when 4 then pd_appl_tracking.step_4   
				when 3 then pd_appl_tracking.step_3   
				when 2 then pd_appl_tracking.step_2  
		END as complete_date, 
         facility.facility_name,   
         v_full_name.full_name   
    FROM pd_appl_tracking,   
         app_hdr,   
         facility,   
         v_full_name,   
         sys_appl_tracker  
   WHERE ( pd_appl_tracking.app_id = app_hdr.app_id ) and  
         ( pd_appl_tracking.prac_id = v_full_name.prac_id ) and  
         ( pd_appl_tracking.facility_id = facility.facility_id ) and  
         ( pd_appl_tracking.active_status = 1) AND 
		( pd_appl_tracking.assigned_user = :as_user )  AND
         ( pd_appl_tracking.days_total is not null )    
" arguments=(("as_user", string))  sort="app_hdr_application_name A complete_date D " )

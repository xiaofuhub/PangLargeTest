table(column=(type=char(255) updatewhereclause=yes name=data_view_app_audit_reference_value dbname="data_view_app_audit.reference_value" dbalias=".reference_value" )
 column=(type=decimal(0) updatewhereclause=yes name=pd_app_audit_prac_id dbname="pd_app_audit.prac_id" dbalias=".prac_id" )
 column=(type=long updatewhereclause=yes name=pd_app_audit_facility_id dbname="pd_app_audit.facility_id" dbalias=".facility_id" )
 column=(type=char(12) updatewhereclause=yes name=rec_type dbname="rec_type" )
 column=(type=char(10) updatewhereclause=yes name=assigned_user dbname="assigned_user" )
 column=(type=char(32766) updatewhereclause=no name=pd_app_audit_notes dbname="pd_app_audit.notes" dbalias=".notes" )
 column=(type=char(50) updatewhereclause=yes name=data_view_screen_screen_alias dbname="data_view_screen.screen_alias" dbalias=".screen_alias" )
 column=(type=decimal(0) updatewhereclause=yes name=pd_app_audit_appt_stat_id dbname="pd_app_audit.appt_stat_id" dbalias=".appt_stat_id" )
 column=(type=char(50) updatewhereclause=yes name=pd_app_audit_identifying_number dbname="pd_app_audit.identifying_number" dbalias=".identifying_number" )
 retrieve="  SELECT data_view_app_audit.description as reference_value,   
         pd_app_audit.prac_id,   
         pd_app_audit.facility_id,   
         rec_type = 'Checklist',   
         assigned_user = '',   
         pd_app_audit.notes,   
         data_view_screen.screen_alias,   
         pd_app_audit.appt_stat_id,   
         pd_app_audit.identifying_number  
    FROM {oj data_view_screen LEFT OUTER JOIN data_view_app_audit ON data_view_screen.screen_id = data_view_app_audit.screen_id},   
         pd_app_audit  
   WHERE ( data_view_app_audit.app_audit_id = pd_app_audit.app_audit_id ) and  
         ( ( data_view_screen.data_view_id = 1 ) AND  
         ( date_completed Is null ) )   

union

  SELECT verif_info.reference_value,  
         verif_info.prac_id,   
   		verif_info.facility_id,
 	 	rec_type = 'Verification',
         verif_info.priority_user as assigned_user,   
 	 	verif_info.notes, 	
         data_view_screen.screen_alias, 
      	verif_info.rec_id,
         verif_info.doc_id
    FROM verif_info,   
         data_view_screen 
   WHERE ( verif_info.screen_id = data_view_screen.screen_id ) and  
         ( data_view_screen.data_view_id = 1 ) AND  
		verif_info.active_status = 1 and
         verif_info.response_code is null 
" )

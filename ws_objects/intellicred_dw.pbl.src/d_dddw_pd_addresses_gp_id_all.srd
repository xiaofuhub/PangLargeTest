         pd_address.city,   
         pd_address.zip,   
         pd_address.rec_id,   
         pd_address.tax_id,   
         pd_address.suite_apart,   
         code_lookup.code,   
         pd_address.gp_id,   
         group_practice.alias_name,   
         pd_address.street ,

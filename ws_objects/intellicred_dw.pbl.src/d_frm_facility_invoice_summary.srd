 retrieve="  SELECT DISTINCT pd_invoice.rec_id,   
         pd_invoice.prac_id,   
         pd_invoice.facility_id,   
         pd_invoice.inv_date,   
         pd_invoice.due_date,   
         pd_invoice.tax_a,   
         pd_invoice.amt_billed,   
         pd_invoice.invc_amt,   
         pd_invoice.amt_paid,   
         pd_invoice.paid,   
         pd_invoice.note,   
         facility.facility_name,   
         v_full_name.full_name,   
         inv_company.comp_name,   
         inv_company.address,   
         inv_company.term_label,   
         inv_company.term_days,   
         facility.street,   
         facility.street_2,   
         facility.city,   
         facility.state,   
         facility.zip,   
         inv_company.phone,   
         inv_company.fax,   
         inv_company.penalty  
    FROM pd_invoice,   
         facility,   
         v_full_name,   
         inv_company  
   WHERE ( pd_invoice.facility_id = facility.facility_id ) and  
         ( pd_invoice.prac_id = v_full_name.prac_id ) and  
         ( ( pd_invoice.facility_id = :facid ) AND  
         ( convert(datetime, convert ( varchar, pd_invoice.inv_date,102 ) ) >= :from_date ) AND  
         ( convert(datetime, convert ( varchar, pd_invoice.inv_date,102 ) ) <= :to_date ) )  

union

  SELECT DISTINCT pd_invoice.rec_id,   
         pd_invoice.prac_id,   
         pd_invoice.facility_id,   
         pd_invoice.inv_date,   
         pd_invoice.due_date,   
         pd_invoice.tax_a,   
         pd_invoice.amt_billed,   
         pd_invoice.invc_amt,   
         pd_invoice.amt_paid,   
         pd_invoice.paid,   
         pd_invoice.note,   
         facility.facility_name,   
         v_full_name.full_name,   
         inv_company.comp_name,   
         inv_company.address,   
         inv_company.term_label,   
         inv_company.term_days,   
         facility.street,   
         facility.street_2,   
         facility.city,   
         facility.state,   
         facility.zip,   
         inv_company.phone,   
         inv_company.fax,   
         inv_company.penalty  
    FROM pd_invoice,   
         facility,   
         v_full_name,   
         inv_company  
   WHERE ( pd_invoice.facility_id = facility.facility_id ) and  
         ( pd_invoice.prac_id = v_full_name.prac_id ) and  
         ( ( pd_invoice.facility_id = :facid ) AND  
         ( pd_invoice.paid <> 1) and   
         ( convert(datetime, convert(varchar,pd_invoice.inv_date,102)) <= :to_date ) )  

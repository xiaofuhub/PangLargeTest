 retrieve="  SELECT verif_info.rec_id,   
         verif_info.prac_id,   
         verif_info.facility_id,   
         verif_info.seq_no,   
         verif_info.screen_id,   
         verif_info.date_recieved,   
         verif_info.response_code,   
         verif_info.active_status,   
         verif_info.reference_value,   
         verif_info.doc_id,   
         verif_info.fee  
    FROM verif_info,   
         pd_affil_stat  
   WHERE ( pd_affil_stat.prac_id = verif_info.prac_id ) and  
        ( (( pd_affil_stat.parent_facility_id = verif_info.facility_id ) and  ( verif_info.screen_id = 1  )) OR  (( pd_affil_stat.verifying_facility = verif_info.facility_id ) and    ( verif_info.screen_id > 1 ) )) and
          ( pd_affil_stat.parent_facility_id = :facility_id ) AND
		 ( verif_info.date_recieved >= :ad_from ) AND  
         ( verif_info.date_recieved <= :ad_to ) AND
	(	verif_info.active_status = 1) AND 
 		( pd_affil_stat.active_status in (1,4)) AND
         ( verif_info.fee > 0 )     
" arguments=(("ad_from", datetime),("ad_to", datetime),("facility_id", number))  sort="prac_id A reference_value A " )

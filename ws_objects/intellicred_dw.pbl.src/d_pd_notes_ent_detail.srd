         pd_notes.prac_id,   
         pd_notes.facility_id,   
         pd_notes.ent_date,   
         pd_notes.user_id,   
         pd_notes.note,   
         facility.facility_name,   
         v_full_name.full_name,   
         pd_notes.note_type,   
         pd_notes.payor_id,   
         pd_notes.group_id,   
         pd_notes.location_id ,
		pd_notes.org_id  
    FROM {oj pd_notes LEFT OUTER JOIN facility ON pd_notes.facility_id = facility.facility_id},   
         v_full_name  
   WHERE (( pd_notes.prac_id = v_full_name.prac_id ) or (pd_notes.prac_id = 0)) and  

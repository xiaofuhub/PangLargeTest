 column=(type=char(35) updatewhereclause=yes name=city dbname="facility.city" dbalias=".city" )
 column=(type=char(2) updatewhereclause=yes name=state dbname="facility.state" dbalias=".state" )
 column=(type=char(12) updatewhereclause=yes name=zip dbname="facility.zip" dbalias=".zip" )
 column=(type=long updatewhereclause=yes name=pd_affil_stat_active_status dbname="pd_affil_stat.active_status" dbalias=".active_status" )
 retrieve="  SELECT facility.facility_id,   
         facility.facility_name,   
         selected = 0,   
         facility.street,   
         facility.street_2,   
         facility.city,   
         facility.state,   
         facility.zip  ,
		pd_affil_stat.active_status
    FROM {oj facility LEFT OUTER JOIN pd_affil_stat ON facility.facility_id = pd_affil_stat.parent_facility_id and  pd_affil_stat.prac_id = :al_prac and  pd_affil_stat.active_status in (1,4,3)},   
         security_user_facilities  
   WHERE ( facility.facility_id = security_user_facilities.facility_id ) and  
         ( ( security_user_facilities.user_id = :as_user ) AND   
         ( security_user_facilities.access_rights = 1 ) )    
" arguments=(("al_prac", number),("as_user", string))  sort="facility_name A " )

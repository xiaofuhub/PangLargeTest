table(column=(type=long updatewhereclause=yes name=data_view_screen_screen_id dbname="data_view_screen.screen_id"  )
column=(type=char(50) updatewhereclause=yes name=data_view_screen_screen_alias dbname="data_view_screen.screen_alias"  )
column=(type=char(255) updatewhereclause=yes name=data_view_fields_field_label dbname="data_view_fields.field_label"  )
column=(type=char(50) updatewhereclause=yes name=field_name_allias dbname="sys_fields.field_name_allias"  )
column=(type=long updatewhereclause=yes name=table_id dbname="sys_fields.table_id"  )
column=(type=long updatewhereclause=yes name=field_id dbname="sys_fields.field_id"  )
column=(type=char(1) updatewhereclause=yes name=lookup_type dbname="sys_fields.lookup_type"  )
column=(type=long updatewhereclause=yes name=field_order dbname="sys_fields.field_order"  )
column=(type=long updatewhereclause=yes name=parent_field dbname="parent_field"  )
column=(type=char(2) updatewhereclause=yes name=field_type dbname="field_type"  )
column=(type=long updatewhereclause=yes name=keep dbname="keep"  )
column=(type=char(50) updatewhereclause=yes name=field_name dbname="sys_fields.field_name"  )
 retrieve="  SELECT DISTINCT
			data_view_screen.screen_id,
			data_view_screen.screen_alias,  
         data_view_fields.field_label,
         sys_fields.field_name_allias,
			sys_fields.table_id,
         sys_fields.field_id,      
         sys_fields.lookup_type,         
         sys_fields.field_order,
         parent_field = 0,
         field_type = 'SS',
         keep = 1,
			sys_fields.field_name
    FROM sys_fields,   
         sys_tables,   
         data_view_fields,   
         data_view_screen 
   WHERE ( sys_fields.table_id = sys_tables.table_id ) and  
         ( sys_fields.field_id = data_view_fields.field_id ) and  
         ( data_view_screen.table_id = sys_tables.table_id ) and  
         ( data_view_screen.data_view_id = data_view_fields.data_view_id and
			  data_view_screen.screen_id = data_view_fields.screen_id and
			  data_view_fields.visible = 'Y'  and 
           (data_view_screen.data_view_id = :al_default_view or (data_view_screen.screen_id in ( 80,81,82,83,84,85,86,43)))
			)
UNION ALL
  SELECT DISTINCT
          no_field_screen.screen_id,
          no_field_screen.screen_alias,  
          sys_fields.field_name_allias,
         sys_fields.field_name_allias,
         sys_fields.table_id,
         sys_fields.field_id,      
         sys_fields.lookup_type,         
         sys_fields.field_order,
         parent_field = 0,
         field_type = 'SS',
         keep = 1,
         sys_fields.field_name
    FROM sys_fields,
         sys_tables,
         (select data_view_screen.table_id, data_view_screen.screen_id, data_view_screen.screen_alias from data_view_screen LEFT OUTER JOIN data_view_fields on 
                (data_view_screen.data_view_id = data_view_fields.data_view_id and data_view_screen.screen_id = data_view_fields.screen_id)
          where (data_view_fields.data_view_id is null) and
                (data_view_screen.data_view_id = :al_default_view or (data_view_screen.screen_id in ( 80,81,82,83,84,85,86,43)))
          ) no_field_screen
   WHERE (sys_fields.table_id = sys_tables.table_id) and
         (sys_tables.table_id = no_field_screen.table_id)
"
  arguments=(("al_default_view", number))
 sort="data_view_screen_screen_alias A, data_view_fields_field_label A"
)

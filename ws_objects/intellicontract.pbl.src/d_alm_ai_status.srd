 retrieve="  SELECT DISTINCT curdate = getdate(),   
         ~"ctx_ai_notification~".~"ctx_action_item_id~",   
         ~"ctx_ai_notification~".~"alm_table~",   
         ~"ctx_ai_notification~".~"alm_column~",   
         ~"ctx_ai_notification~".~"alm_message~",   
         ~"ctx_ai_notification~".~"alm_enabled~",   
         ~"ctx_ai_notification~".~"alm_notification_days~",   
         ~"ctx_ai_notification~".~"alm_last_notification_sent~",   
         ~"ctx_ai_notification~".~"alm_total_tries~",   
         ~"ctx_ai_notification~".~"alm_total_tries_counter~",   
         ~"ctx_ai_notification~".~"alm_frequency~",   
         ~"ctx_ai_notification~".~"create_action_item~",   
         ~"ctx_ai_notification~".~"record_id~",   
         ~"ctx_ai_notification~".~"ctx_id~",   
         ~"ctx_action_items~".~"action_date~",   
         ~"ctx_action_items~".~"due_date~",   
         ~"ctx_ai_notification~".~"wf_id~",   
         ~"ctx_ai_notification~".~"alm_last_wf_trigger~",   
         ~"ctx_ai_notification~".~"alm_email_template_id~",   
         ~"ctx_ai_notification~".~"alm_attachment_name~",   
         alm_message_length = datalength(alm_message_blob),   
         alm_attachment_length = datalength(alm_attachment),   
         email_notification = 0,   
         wf_trigger = 0,   
         alm_mailto = Replace(~"ctx_ai_notification~".~"alm_mailto~" ,'$<OwnerEmail>$', isnull( ~"security_users~".~"email_id~" ,'')    ) ,   
         ~"ctx_ai_notification~".~"auto_sign~",   
         alm_create = Replace(~"ctx_ai_notification~".~"alm_create~", '$<Owner>$', Isnull(~"ctx_contacts~".~"user_d~",'')), 
         ~"ctx_ai_notification~".~"alm_subject~",   
         ~"ctx_basic_info~".~"category~",   
         ~"app_facility~".~"facility_name~",   
         ~"ctx_basic_info~".~"ctx_id~",   
         ~"ctx_contacts~".~"user_d~",   
         alm_column_name = 'Due Date',   
         ~"ctx_ai_notification~".~"alm_date~",   
         ~"ctx_ai_notification~".~"alm_appointment~",   
         ~"security_users~".~"email_id~"  
    FROM {oj ~"ctx_basic_info~" LEFT OUTER JOIN ~"app_facility~" ON ~"ctx_basic_info~".~"app_facility~" = ~"app_facility~".~"facility_id~"}, {oj ~"ctx_contacts~" LEFT OUTER JOIN ~"security_users~" ON ~"ctx_contacts~".~"user_d~" = ~"security_users~".~"user_id~"},   
         ~"code_lookup~",   
         ~"ctx_ai_notification~",   
         ~"ctx_action_items~",   
         ~"ctx_contract_contacts~"  
   WHERE ( ~"ctx_basic_info~".~"status~" = ~"code_lookup~".~"lookup_code~" ) and  
         ( ~"ctx_basic_info~".~"ctx_id~" = ~"ctx_ai_notification~".~"ctx_id~" ) and  
         ( ~"ctx_action_items~".~"ctx_action_item_id~" = ~"ctx_ai_notification~".~"ctx_action_item_id~" ) and  
         ( ~"ctx_action_items~".~"ctx_id~" = ~"ctx_ai_notification~".~"ctx_id~" ) and  
         ( ~"ctx_contract_contacts~".~"contact_id~" = ~"ctx_contacts~".~"contact_id~" ) and  
         ( ~"ctx_contract_contacts~".~"ctx_id~" = ~"ctx_basic_info~".~"ctx_id~" ) and  
         ( ( code_lookup.lookup_name = 'Contract Status' ) AND  
         ( code_lookup.custom_1 = 'YES' ) AND  
         ( ctx_ai_notification.alm_enabled = '1' ) AND  
         ( ctx_basic_info.is_template = 0 ) AND  
         (ctx_ai_notification.alm_total_tries_counter is null OR  
         ctx_ai_notification.alm_total_tries <> ctx_ai_notification.alm_total_tries_counter) AND  
         DATEDIFF(day, getdate(),ctx_action_items.due_date)  <= ctx_ai_notification.alm_notification_days AND  
         ctx_contract_contacts.owner = 'Y' AND  
         ctx_contract_contacts.ic_area = 'I' )   
ORDER BY ~"ctx_basic_info~".~"ctx_id~" ASC   
" update="ctx_ai_notification" updatewhere=0 updatekeyinplace=no )

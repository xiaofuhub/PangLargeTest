      ( ~"ctx_am_action_item~".~"seq_id~" = ~"ctx_am_ai_notification~".~"seq_id~" ) and    
      ( ~"ctx_contract_contacts~".~"contact_id~" = ~"ctx_contacts~".~"contact_id~" ) and      
    ( ~"ctx_contract_contacts~".~"ctx_id~" = ~"ctx_basic_info~".~"ctx_id~" ) and      
    ( ( code_lookup.lookup_name = 'Contract Status' ) and   
       ( code_lookup.custom_1 = 'YES' ) and          ( ctx_am_ai_notification.alm_enabled = '1' ) and        
  ( ctx_basic_info.is_template = 0 ) and       
   (ctx_am_ai_notification.alm_total_tries_counter is null or   ( ctx_am_ai_notification.alm_total_tries <> ctx_am_ai_notification.alm_total_tries_counter) ) and      
    ( DATEDIFF(day, getdate(),ctx_am_action_item.due_date)  <= ctx_am_ai_notification.alm_notification_days ) and   
       ( ctx_contract_contacts.owner = 'Y' ) and     
     ( ctx_contract_contacts.ic_area = 'I' ) )
 ORDER BY ~"ctx_basic_info~".~"ctx_id~"          ASC  " update="ctx_am_ai_notification" updatewhere=0 updatekeyinplace=no )

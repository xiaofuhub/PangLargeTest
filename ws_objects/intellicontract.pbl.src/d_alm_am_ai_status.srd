     ~"ctx_am_ai_notification~".~"record_id~" ,           ~"ctx_am_action_item~".~"status_date~" ,           ~"ctx_am_action_item~".~"due_date~" ,           ~"ctx_am_ai_notification~".~"wf_id~" ,   
        ~"ctx_am_ai_notification~".~"alm_last_wf_trigger~" ,           ~"ctx_am_document~".~"ctx_id~" ,           ~"ctx_am_ai_notification~".~"alm_email_template_id~" ,       
    ~"ctx_am_ai_notification~".~"alm_attachment_name~" ,    
       wf_trigger = 0,           email_notification = 0,           alm_message_length = datalength(alm_message_blob),        
   alm_attachment_length = datalength(alm_attachment),          
alm_mailto = Replace(~"ctx_am_ai_notification~".~"alm_mailto~" ,'$<OwnerEmail>$', isnull( ~"security_users~".~"email_id~" ,'')    ) ,
      ~"ctx_am_document~".~"doc_name~" ,         
  ~"ctx_am_ai_notification~".~"auto_sign~" ,           
alm_create = Replace(~"ctx_am_ai_notification~".~"alm_create~", '$<Owner>$', Isnull(~"ctx_contacts~".~"user_d~",'')),
        ~"ctx_am_ai_notification~".~"alm_subject~" ,           ~"ctx_basic_info~".~"category~" ,    
       ~"app_facility~".~"facility_name~" ,           ~"ctx_basic_info~".~"ctx_id~" ,           ~"ctx_contacts~".~"user_d~" ,           alm_column_name = 'Due Date',           ~"ctx_am_ai_notification~".~"alm_date~" ,     
      ~"ctx_am_ai_notification~".~"alm_appointment~" ,
security_users.email_id
 FROM {oj ~"ctx_basic_info~" LEFT OUTER JOIN ~"app_facility~" ON ~"ctx_basic_info~".~"app_facility~" = ~"app_facility~".~"facility_id~"} ,       
    ~"code_lookup~" ,           ~"ctx_am_ai_notification~" ,     
      ~"ctx_am_document~" ,           ~"ctx_am_action_item~" ,        
           ~"ctx_contract_contacts~"   ,
 ~"ctx_contacts~" Left Outer Join security_users On (ctx_contacts.user_d = security_users.user_id)
 WHERE ( ~"ctx_basic_info~".~"status~" = ~"code_lookup~".~"lookup_code~" ) and     
     ( ~"ctx_am_document~".~"ctx_id~" = ~"ctx_basic_info~".~"ctx_id~" ) and   
       ( ~"ctx_am_ai_notification~".~"doc_id~" = ~"ctx_am_document~".~"doc_id~" ) and   

 retrieve="  SELECT ctx_contract_contacts.ctx_id,   
         ctx_contract_contacts.contact_id,   
         ctx_contract_contacts.association,   
         ctx_contacts.last_name,   
         ctx_contacts.first_name,   
         ctx_contacts.middle_name,   
         ctx_contract_contacts.ic_area,   
         ctx_contacts.contact_type,   
         ctx_contract_contacts.record_id,   
         ctx_contract_contacts.default_email,   
         ctx_contract_contacts.owner,   
         ctx_contract_contacts.access,
			ctx_contract_contacts.custom_n1,
			ctx_contract_contacts.custom_n2,
			ctx_contract_contacts.custom_n3,
			ctx_contract_contacts.custom_percent_1,
			ctx_contract_contacts.custom_percent_2,
			ctx_contract_contacts.custom_1,
			ctx_contract_contacts.custom_2,
			ctx_contract_contacts.custom_3,
         ctx_contacts.contact_title,   
         ctx_contacts.contact_department,   
         ctx_contacts.user_d,   
			app_facility.contact_type2,
			ctx_contacts.facility_id,
			app_facility.facility_name,
          contact_phone = (select top 1 A.phone 
									from ctx_contacts_numbers A 
								   where A.contact_id =  ctx_contract_contacts.contact_id and
											a.via = (select top 1 lookup_code from code_lookup where 
                                        lookup_name = 'Contract Contact Via' and code = 'Phone' ))  ,
			 phone_ext = (select top 1 A.ext
									from ctx_contacts_numbers A 
								   where A.contact_id =  ctx_contract_contacts.contact_id and
											a.via = (select top 1 lookup_code from code_lookup where 
                                        lookup_name = 'Contract Contact Via' and code = 'Phone' ))  ,
          contact_email = (select top 1 A.contact_email 
									from ctx_contacts_numbers A 
								   where A.contact_id =  ctx_contract_contacts.contact_id and
											a.via = (select top 1 lookup_code from code_lookup where 
                                        lookup_name = 'Contract Contact Via' and code = 'Email' ))
,ctx_contract_contacts.association2, ctx_contract_contacts.association3,ctx_contract_contacts.association4,
ctx_contract_contacts.association5,ctx_contract_contacts.association6,ctx_contract_contacts.association7,
ctx_contract_contacts.association8 ,ctx_contract_contacts.association9,ctx_contract_contacts.association10
  FROM  ctx_contract_contacts,ctx_contacts,app_facility
   WHERE ( ctx_contract_contacts.contact_id = ctx_contacts.contact_id ) and  
			(ctx_contacts.facility_id = app_facility.facility_id) and 
         ( ( ctx_contract_contacts.ctx_id = :al_ctx_id ) AND  
         ( ctx_contract_contacts.ic_area = :as_ic_area ) )
" update="ctx_contract_contacts" updatewhere=0 updatekeyinplace=no arguments=(("al_ctx_id", number),("as_ic_area", string)) )

 retrieve="  SELECT ctx_contract_contacts.ctx_id,
		 ctx_contract_contacts.contact_id,
		 ctx_contract_contacts.ic_area,
		 ctx_contract_contacts.owner,
		ctx_contract_contacts.default_email,
         ctx_contacts.user_d

    FROM ctx_contract_contacts
	INNER JOIN ctx_contacts ON (ctx_contract_contacts.contact_id = ctx_contacts.contact_id)
   WHERE ( ctx_contract_contacts.owner = 'Y' ) AND  
         ( ctx_contract_contacts.ic_area = 'I' ) AND
		 ( ctx_contacts.user_d = :as_user)  AND
         ( (:al_all = 1) or (:al_all = 0 and ctx_contract_contacts.ctx_id in ( :al_ctx_id )) )   " update="ctx_contract_contacts" updatewhere=0 updatekeyinplace=yes arguments=(("al_ctx_id", numberlist),("as_user", string),("al_all", number)) )

is_sql_statements[UpperBound(is_sql_statements) + 1] =  &
	"if exists (select TOP 1 1 from  sysobjects where  name='v_group_covering' and type = 'V')  " + &
	"  drop view v_group_covering "

is_sql_statements[UpperBound(is_sql_statements) + 1] =  &
"CREATE VIEW v_group_covering  "+ &
"AS   "+ &
"SELECT DISTINCT TOP 100 PERCENT * "+ &
"FROM   ( "+ &
"           SELECT pd_group_providers.facility_id, "+ &
"                  pd_group_providers.provider_name, "+ &
"                  code_lookup_b.description AS specialty, "+ &
"                  code_lookup_b.description AS specialty_code, "+ &
"                  code_lookup_a.code AS covering, "+ &
"                  code_lookup_a.description AS covering_description, "+ &
"                  pd_group_providers.covering_days, "+ &
"                  pd_group_providers.custom_1, "+ &
"                  pd_group_providers.custom_2, "+ &
"                  pd_group_providers.custom_3, "+ &
"                  pd_group_providers.custom_4, "+ &
"                  pd_group_providers.custom_5, "+ &
"                  facility.facility_name, "+ &
"                  pd_group_providers.prac_id, "+ &
"                  pd_group_providers.rec_id, "+ &
"                  pd_group_providers.custom_6, "+ &
"                  pd_group_providers.custom_7, "+ &
"                  pd_group_providers.custom_8, "+ &
"                  pd_group_providers.custom_9, "+ &
"                  pd_group_providers.custom_10, "+ &
"                  pd_group_providers.custom_d1, "+ &
"                  pd_group_providers.custom_d2, "+ &
"                  pd_group_providers.linked_prac_id, "+ &
"                  pd_group_providers.custom_11, "+ &
"                  pd_group_providers.custom_12, "+ &
"                  pd_group_providers.custom_13, "+ &
"                  pd_group_providers.custom_14, "+ &
"                  pd_group_providers.custom_15, "+ &
"                  pd_group_providers.custom_16, "+ &
"                  pd_group_providers.custom_17, "+ &
"                  pd_group_providers.custom_18, "+ &
"                  pd_group_providers.custom_19, "+ &
"                  pd_group_providers.custom_20 "+ &
"           FROM   pd_group_providers "+ &
"                  INNER JOIN facility "+ &
"                       ON  pd_group_providers.facility_id = facility.facility_id "+ &
"                  LEFT OUTER JOIN code_lookup code_lookup_a "+ &
"                       ON  pd_group_providers.covering = code_lookup_a.lookup_code "+ &
"                  LEFT OUTER JOIN code_lookup code_lookup_b "+ &
"                       ON  pd_group_providers.specialty = code_lookup_b.lookup_code "+ &
"           WHERE  EXISTS ( "+ &
"                      SELECT 1 "+ &
"                      FROM   sys_tables "+ &
"                      WHERE  table_id = 29 "+ &
"                             AND facility_specific = 1 "+ &
"                  ) "+ &
"           UNION ALL "+ &
"           SELECT pd_group_providers.facility_id, "+ &
"                  pd_group_providers.provider_name, "+ &
"                  code_lookup_b.description AS specialty, "+ &
"                  code_lookup_b.description AS specialty_code, "+ &
"                  code_lookup_a.code AS covering, "+ &
"                  code_lookup_a.description AS covering_description, "+ &
"                  pd_group_providers.covering_days, "+ &
"                  pd_group_providers.custom_1, "+ &
"                  pd_group_providers.custom_2, "+ &
"                  pd_group_providers.custom_3, "+ &
"                  pd_group_providers.custom_4, "+ &
"                  pd_group_providers.custom_5, "+ &
"                  '' AS facility_name, "+ &
"                  pd_group_providers.prac_id, "+ &
"                  pd_group_providers.rec_id, "+ &
"                  pd_group_providers.custom_6, "+ &
"                  pd_group_providers.custom_7, "+ &
"                  pd_group_providers.custom_8, "+ &
"                  pd_group_providers.custom_9, "+ &
"                  pd_group_providers.custom_10, "+ &
"                  pd_group_providers.custom_d1, "+ &
"                  pd_group_providers.custom_d2, "+ &
"                  pd_group_providers.linked_prac_id, "+ &
"                  pd_group_providers.custom_11, "+ &
"                  pd_group_providers.custom_12, "+ &
"                  pd_group_providers.custom_13, "+ &
"                  pd_group_providers.custom_14, "+ &
"                  pd_group_providers.custom_15, "+ &
"                  pd_group_providers.custom_16, "+ &
"                  pd_group_providers.custom_17, "+ &
"                  pd_group_providers.custom_18, "+ &
"                  pd_group_providers.custom_19, "+ &
"                  pd_group_providers.custom_20 "+ &
"           FROM   pd_group_providers "+ &
"                  LEFT OUTER JOIN code_lookup code_lookup_a "+ &
"                       ON  pd_group_providers.covering = code_lookup_a.lookup_code "+ &
"                  LEFT OUTER JOIN code_lookup code_lookup_b "+ &
"                       ON  pd_group_providers.specialty = code_lookup_b.lookup_code "+ &
"           WHERE  NOT EXISTS ( "+ &
"                      SELECT 1 "+ &
"                      FROM   sys_tables "+ &
"                      WHERE  table_id = 29 "+ &
"                             AND facility_specific = 1 "+ &
"                  ) "+ &
"       )a "+ &
"ORDER BY "+ &
"       facility_id  "
//------------------- APPEON END ------------------- 


"    pd_affil_stat.priority,pd_affil_stat.priority_user, " + &
"    pd_affil_stat.custom_1, " + &
"    pd_affil_stat.custom_2, " + &
"    pd_affil_stat.custom_3, " + &
"    pd_affil_stat.custom_4, " + &
"    pd_affil_stat.custom_5, " + &
"    pd_affil_stat.prov_status, " + &

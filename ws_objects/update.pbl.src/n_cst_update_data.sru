public subroutine of_generate_report_data ();//
datastore lds_dw
long		 ll_i,ll_count
string	 ls_prfdw

//
select count(*) into :ll_count from data_view_report_fields where report_from='P';
if ll_count > 0 then return

lds_dw=create datastore
lds_dw.dataobject='d_profile_report_dw'
lds_dw.settransobject(sqlca)
ll_count=lds_dw.retrieve()

IF Not IsValid( w_infodisp ) THEN Open(w_infodisp)
IF IsValid(w_infodisp) THEN w_infodisp.Title = 'Generating Report Data, Please stand by'
IF IsValid(w_infodisp) THEN w_infodisp.Center = True
IF IsValid(w_infodisp) THEN w_infodisp.st_complete.Visible = False
IF IsValid(w_infodisp) THEN w_infodisp.st_3.Visible = False
IF IsValid(w_infodisp) THEN w_infodisp.st_information.Visible = False
IF IsValid(w_infodisp) THEN w_infodisp.st_1.Text = 'Generating Report Data, Please stand by'
IF IsValid(w_infodisp) THEN w_infodisp.wf_set_min_max(1,ll_count)

for ll_i=1 to ll_count
	IF IsValid(w_infodisp) THEN
		Yield()
		w_infodisp.wf_step_pbar(1)
		w_infodisp.st_1.Text = 'Generate ' + String(ll_i) + ' of ' + String(ll_count)+' Please stand by!'
	END IF
	ls_prfdw=lds_dw.getitemstring(ll_i,"prf_dw")

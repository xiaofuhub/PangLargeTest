		"    from   #table_Column as t1,#tablePK as t2  " + &
		"    where t1.FTableName = t2.FTableName and " + &
		"           t1.FTableOwner = t2.FTableOwner and " + &
		"        t2.FObjectType = 'PK' and " + &
		"        t1.FColumnName = t2.FColumnName " + &
		"  update #table_column as t1 set " + &
		"    FIsUI = 'Y'  " + &
		"    from    #table_Column as t1,#tablePK as t2  " + &
		"    where    t1.FTableName = t2.FTableName and " + &
		"    t1.FTableOwner = t2.FTableOwner and " + &
		"    t2.FObjectType = 'UI' and " + &
		"    t1.FColumnName = t2.FColumnName " + &
		"  update #table_Column set " + &
		"    FIsComputeColumn = 'Y' where isnull(FTableName,'') = '' " + &
		"     " + &
		"  " + &
		"  if(select count(*) from #tablePK) > 0 " + &
		"    select distinct ResultSetType='PK_UI',FOriginalTableName,FObjectType,FColumnName from " + &
		"      #tablePK " + &
		"  else " + &
		"    select ResultSetType='NoPK_UI' " + &
		"   " + &
		"  " + &
		"  if exists(select 1 from sys.systable where table_name = 'pbcattbl' and creator = 1)  " + &
		"    if(select count(*) from dba.pbcattbl as t1,#table_column as t2 where " + &
		"        t1.pbt_tnam = t2.FTableName and " + &
		"        t1.pbt_ownr = t2.FTableOwner and " + &
		"        t2.FOriginalTableName <> '') > 0 " + &
		"      select distinct ResultSetType='TableExtent',t2.FOriginalTableName,t2.FDBName,t1.* from " + &
		"        dba.pbcattbl as t1,#table_column as t2 where " + &
		"        t1.pbt_tnam = t2.FTableName and " + &
		"        t1.pbt_ownr = t2.FTableOwner and " + &
		"        t2.FOriginalTableName <> '' " + &
		"    else " + &
		"      select ResultSetType='NoTableExtent' " + &
		"  else " + &
		"    select ResultSetType='NoTableExtent' " + &
		"   " + &
		"  if exists(select 1 from sys.systable where table_name = 'pbcatcol' and creator = 1) " + &
		"    if(select count(*) from dba.pbcatcol as t1,#table_column as t2 where " + &
		"        t1.pbc_tnam = t2.FTableName and " + &
		"        t1.pbc_ownr = t2.FTableOwner and " + &
		"        t1.pbc_cnam = t2.FColumnName ) > 0 " + &
		"      update #table_column as t2 set " + &
		"        pbc_labl = t1.pbc_labl, " + &
		"        pbc_lpos = t1.pbc_lpos, " + &
		"        pbc_hdr = t1.pbc_hdr, " + &
		"        pbc_hpos = t1.pbc_hpos, " + &
		"        pbc_jtfy = t1.pbc_jtfy, " + &
		"        pbc_mask = t1.pbc_mask, " + &

//2. for Encrypt Password - Scofield 2008-11-11
String	ls_Password
long		ll_Cycle,ll_Count,ll_ColLen,ll_EnKeyVal

Datastore 		lds_UserPass,lds_EmailPass
n_cst_Encrypt 	lnvo_Encrypt

if gs_DBType = "ASA" then
	SELECT sys.syscolumn.width
	  INTO :ll_ColLen
  	  FROM sys.syscolumn,sys.systable
  	 WHERE sys.syscolumn.table_id = sys.systable.table_id
  	   AND sys.systable.table_name = 'em_smtp_accounts'
  	   AND sys.syscolumn.column_name = 'password' ;
else
	SELECT syscolumns.length
	  INTO :ll_ColLen
     FROM sysobjects,syscolumns
    WHERE sysobjects.id = syscolumns.id and 
          sysobjects.xtype = 'U' and 
          sysobjects.name = 'em_smtp_accounts' and 
          syscolumns.name = 'password' ;
end if

if ll_ColLen < 100 then
	if gs_DBType = "ASA" then
		is_sql_statements[upperbound(is_sql_statements) + 1] =  "ALTER TABLE em_smtp_accounts MODIFY password varchar(100)"
	else
		is_sql_statements[upperbound(is_sql_statements) + 1] =  "ALTER TABLE em_smtp_accounts ALTER COLUMN password varchar(100) null"
	end if
end if

SELECT set_45 INTO :ll_EnKeyVal FROM icred_settings;

if ll_EnKeyVal < 126 or ll_EnKeyVal > 160 then
	lds_UserPass = Create DataStore
	lds_UserPass.DataObject = 'd_user_password'
	lds_UserPass.SetTransObject(SQLCA)
	lds_UserPass.Retrieve()
	
	ll_Count = lds_UserPass.RowCount()
	for ll_Cycle = 1 to ll_Count
		ls_Password = lds_UserPass.GetItemString(ll_Cycle,'user_password')
		ls_Password = lnvo_Encrypt.of_Decrypt(ls_Password)
		lds_UserPass.SetItem(ll_Cycle,'user_password',ls_Password)
	next
	
	UPDATE icred_settings SET set_45 = 144;
	
	for ll_Cycle = 1 to ll_Count
		ls_Password = lds_UserPass.GetItemString(ll_Cycle,'user_password')
		ls_Password = lnvo_Encrypt.of_Encrypt(ls_Password)
		lds_UserPass.SetItem(ll_Cycle,'user_password',ls_Password)
	next
	
	if ll_Count > 0 then
		if lds_UserPass.Update() <> 1 then
			ROLLBACK USING SQLCA;
			
			ls_Error += "n_cst_update_data.of_ver91_release_data2() Information: The Encryption Value has been automatically changed from " + String(ll_EnKeyVal) + " to 144 during the upgrading process, if there are users that cannot login the system, please call Support."
			if IsValid(gnv_logservice) then
				gnv_logservice.of_setloglevel(2) //warning level
				gnv_logservice.of_log_warning(ls_Error)
				gb_upgrade_failed = True
			end if
		else
			COMMIT USING SQLCA;
		end if
	end if
	
	Destroy lds_UserPass
end if

if ll_ColLen < 100 then
	lds_EmailPass = Create DataStore
	lds_EmailPass.DataObject = 'd_email_password'
	lds_EmailPass.SetTransObject(SQLCA)
	lds_EmailPass.Retrieve()
		
	ll_Count = lds_EmailPass.RowCount()
	for ll_Cycle = 1 to ll_Count
		ls_Password = lds_EmailPass.GetItemString(ll_Cycle,'password')
		if IsNull(ls_Password) then CONTINUE
		
		ls_Password = lnvo_Encrypt.of_Encrypt(ls_Password)
		lds_EmailPass.SetItem(ll_Cycle,'password',ls_Password)
	next
	
	if ll_Count > 0 then
		if lds_EmailPass.Update() <> 1 then
			ROLLBACK USING SQLCA;
			
			ls_Error+= "n_cst_update_data.of_ver91_release_data2() Information: Encrypt em_smtp_accounts.Password failed, call support."
			if IsValid(gnv_logservice) then
				gnv_logservice.of_setloglevel(2) //warning level
				gnv_logservice.of_log_warning(ls_Error)
				gb_upgrade_failed = True
			end if
		else
			COMMIT USING SQLCA;
		end if
	end if
	
	Destroy lds_EmailPass
end if



table(column=(type=long updatewhereclause=yes name=table_id dbname="sys_tables.table_id" dbalias=".table_id" )
 column=(type=char(128) updatewhereclause=yes name=cname dbname="v_syscolumns.cname" dbalias=".cname" )
 column=(type=char(1) updatewhereclause=yes name=coltype dbname="coltype" )
 column=(type=long updatewhereclause=yes name=length dbname="v_syscolumns.length" dbalias=".length" )
 retrieve="select sys_tables.table_id as table_id, 
       v_syscolumns.cname as cname,
       coltype=case v_syscolumns.coltype
					WHEN 'int'      THEN 'N'    
					WHEN 'float'    THEN 'N'    
					WHEN 'smallint' THEN 'N'    
					WHEN 'money'    THEN 'N'    
					WHEN 'real'     THEN 'N'    
					WHEN 'decimal'  THEN 'N'    
					WHEN 'tinyint'  THEN 'N'    
					WHEN 'numeric'  THEN 'N'    
					WHEN 'bit'      THEN 'N'    
					WHEN 'char'     THEN 'C'    
					WHEN 'nvarchar' THEN 'C'    
					WHEN 'varchar'  THEN 'C'    
					WHEN 'text'     THEN 'C'    
					WHEN 'datetime' THEN 'D'    
                end, 
       v_syscolumns.length as length
from sysobjects,v_syscolumns,sys_tables 
where sysobjects.xtype='U' and 
      sysobjects.id=v_syscolumns.table_id and
      sysobjects.name=sys_tables.table_name COLLATE DATABASE_DEFAULT and
      sysobjects.name in (:table_name) and 
      convert(varchar,sys_tables.table_id)+v_syscolumns.cname COLLATE DATABASE_DEFAULT  not in(select distinct  convert(varchar,table_id)+field_name from sys_fields )
order by sys_tables.table_id, v_syscolumns.colno 
" arguments=(("table_name", stringlist)) )

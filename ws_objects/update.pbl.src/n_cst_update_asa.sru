is_sql_statements[UpperBound(is_sql_statements) + 1] =  &
	"CREATE VIEW  v_qa_hdr " + &
	"AS " + &
	"SELECT        PD_QA_HDR.PD_QA_HDR_ID AS pd_qa_hdr_id, PD_QA_HDR.PRAC_ID AS prac_id, LTRIM(RTRIM(ISNULL(pd_basic.last_name, '')))  " + &

	"FROM         PD_QA_HDR INNER JOIN " + &
	"                      pd_basic ON PD_QA_HDR.PRAC_ID = pd_basic.prac_id INNER JOIN " + &
	"                      QA_PROFILE ON PD_QA_HDR.QA_PROFILE_ID = QA_PROFILE.QA_PROFILE_ID INNER JOIN " + &

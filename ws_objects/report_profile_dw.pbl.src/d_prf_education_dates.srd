 retrieve="  SELECT DISTINCT pd_education.rec_id,   
         pd_education.prac_id,   
			code_lookup_a.description education_type,
			code_lookup_b.description degree,
         pd_education.major,   
         pd_education.month_from,   
         pd_education.year_from,   
         pd_education.month_thru,   
         pd_education.year_thru,   
  			address_lookup_c.entity_name school_code,
         pd_education.cust_1,   
         pd_education.cust_2,   
         pd_education.cust_3,   
         pd_education.cust_4,   
         pd_education.cust_5,   
         pd_education.start_date,   
         pd_education.end_date  
    FROM pd_education  
			LEFT OUTER JOIN code_lookup code_lookup_a  ON pd_education.education_type = code_lookup_a.lookup_code  
			LEFT OUTER JOIN code_lookup code_lookup_b  ON pd_education.degree = code_lookup_b.lookup_code  
			LEFT OUTER JOIN address_lookup address_lookup_c ON pd_education.school_code = address_lookup_c.lookup_code 
   WHERE ( pd_education.prac_id = :prac_id )     
" arguments=(("prac_id", number))  sort="end_date D start_date D " )

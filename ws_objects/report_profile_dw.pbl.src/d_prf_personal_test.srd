 retrieve="  SELECT DISTINCT 
			code_lookup_c.code married,   
	  pd_basic.prac_id, 
         pd_basic.spouse_name,   
         pd_basic.date_of_birth,   
			code_lookup_d.code sex,
         pd_basic.ssn,   
         pd_basic.prac_category,   
         pd_basic.maiden_other_name,   
         pd_basic.pob_city,   
         pd_basic.phone,   
         pd_basic.beeper_1,   
         pd_basic.beeper_2,   
         pd_basic.personal_pager,   
         pd_basic.hospital_pager,   
         pd_basic.car_phone_1,   
         pd_basic.car_phone_2, 
			pd_basic.npi_number, 
			code_lookup_a.code pob_state,   
         code_lookup_b.description pob_country
    FROM {oj pd_basic  
	     LEFT OUTER JOIN code_lookup code_lookup_a  ON pd_basic.pob_state = code_lookup_a.lookup_code} 
		  LEFT OUTER JOIN code_lookup code_lookup_b  ON pd_basic.pob_country = code_lookup_b.lookup_code  
		  LEFT OUTER JOIN code_lookup code_lookup_c  ON pd_basic.married = code_lookup_c.lookup_code  
		  LEFT OUTER JOIN code_lookup code_lookup_d  ON pd_basic.sex = code_lookup_d.lookup_code  

   WHERE ( pd_basic.prac_id = :prac_id )
order by rec_id
" arguments=(("prac_id", number)) )

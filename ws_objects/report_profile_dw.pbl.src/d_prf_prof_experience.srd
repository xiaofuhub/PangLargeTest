 retrieve="  SELECT DISTINCT pd_prof_experience.rec_id,   
         pd_prof_experience.prac_id,   
         pd_prof_experience.date_from_month,   
         pd_prof_experience.date_to_month,   
         pd_prof_experience.organization,
         pd_prof_experience.street_1,
         pd_prof_experience.street_2,   
         pd_prof_experience.city,   
         pd_prof_experience.state,   
         pd_prof_experience.zip,   
         pd_prof_experience.country,   
			code_lookup_c.description title_duties,
         pd_prof_experience.date_from_year,   
         pd_prof_experience.date_to_year,   
         pd_prof_experience.cust_1,   
         pd_prof_experience.cust_2,   
         pd_prof_experience.cust_3,   
         pd_prof_experience.cust_4,   
         pd_prof_experience.cust_5,   
         code_lookup_a.code state_code,   
         code_lookup_b.code count_code,   
         state = code_lookup_a.code,   
         country = code_lookup_b.code  
    FROM pd_prof_experience  
			LEFT OUTER JOIN code_lookup code_lookup_a  ON pd_prof_experience.state = code_lookup_a.lookup_code 
			LEFT OUTER JOIN code_lookup code_lookup_b  ON pd_prof_experience.country = code_lookup_b.lookup_code 
			LEFT OUTER JOIN code_lookup code_lookup_c  ON pd_prof_experience.title_duties = code_lookup_c.lookup_code 
   WHERE ( pd_prof_experience.prac_id = :prac_id )     
" arguments=(("prac_id", number))  sort="pd_prof_experience_date_to_year D pd_prof_experience_date_to_month D " )

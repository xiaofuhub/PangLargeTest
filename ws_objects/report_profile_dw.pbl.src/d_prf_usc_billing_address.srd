 retrieve="  SELECT DISTINCT pd_address.rec_id,   
         pd_address.prac_id,   
         pd_address.office_in_home,   
         pd_address.mailing_address,   
         pd_address.practice_type,   
         pd_address.street,   
         pd_address.city,   
         pd_address.state,   
         pd_address.country,   
         pd_address.pager_number,   
         pd_address.phone,   
         pd_address.ext,   
         pd_address.fax,   
         pd_address.zip,   
         pd_address.county,   
         pd_address.contact_person,   
         pd_address.tax_id,   
         pd_address.tax_id_other_1,   
         pd_address.tax_id_other_2,   
         pd_address.business_mgr,   
         pd_address.office_mgr,   
         pd_address.e_mail_address,   
         pd_address.web_address,   
         code_lookup_a.code,   
         pd_address.street_2  
    FROM {oj pd_address  LEFT OUTER JOIN code_lookup code_lookup_a  ON pd_address.state = code_lookup_a.lookup_code},   
         pd_address_link  
   WHERE ( pd_address.prac_id = pd_address_link.prac_id ) and  
         ( pd_address.rec_id = pd_address_link.address_id ) and  
         ( pd_address.prac_id = :prac_id ) AND  
         pd_address_link.facility_id = :facility_id AND  
         pd_address_link.billing = 1 AND  
         pd_address.active_status = 1    
" arguments=(("facility_id", number),("prac_id", number)) )

table(column=(type=char(9) updatewhereclause=yes name=screen dbname="screen" )
 column=(type=decimal(0) updatewhereclause=yes name=state dbname="pd_dea_state_csr.state" dbalias=".state" )
 column=(type=char(30) updatewhereclause=yes name=code_lookup_code dbname="code_lookup.code" dbalias=".code" )
 column=(type=char(50) updatewhereclause=yes name=license_number dbname="pd_dea_state_csr.license_number" dbalias=".license_number" )
 column=(type=datetime updatewhereclause=yes name=expiration_date dbname="pd_dea_state_csr.expiration_date" dbalias=".expiration_date" )
 retrieve="  SELECT screen = 'DEA',
		pd_dea_state_csr.state, 
		code_lookup.code,  
         pd_dea_state_csr.license_number,   
         pd_dea_state_csr.expiration_date  
    FROM pd_dea_state_csr, code_lookup  
   WHERE 
		pd_dea_state_csr.state = code_lookup.lookup_code  	and 
		pd_dea_state_csr.prac_id = :al_prac    and
		pd_dea_state_csr.active_status = 1 

union

  SELECT  screen = 'Insurance',
		pd_insurance.carrier_code, 
		address_lookup.code, 
         pd_insurance.policy_number,   
         pd_insurance.coverage_to 
    FROM  pd_insurance,   address_lookup
   WHERE
		pd_insurance.carrier_code = address_lookup.lookup_code  	and 
 		pd_insurance.prac_id = :al_prac  and
		pd_insurance.active_status = 1 
union

  SELECT screen = 'License',
		pd_license.state,    
		code_lookup.code, 
         pd_license.license_number,   
         pd_license.expiration_date  
    FROM  pd_license, code_lookup
   WHERE 
		pd_license.state = code_lookup.lookup_code  	and 
		pd_license.active_status = 1 and
		pd_license.prac_id = :al_prac  

union 

  SELECT screen = 'Other ids',
		pd_other_ids.id_type,  
		code_lookup.code,  
         pd_other_ids.id_number,   
         pd_other_ids.exp_date  
    FROM         pd_other_ids, 		code_lookup
   WHERE 
		pd_other_ids.id_type = code_lookup.lookup_code  	and 
		pd_other_ids.prac_id = :al_prac  " arguments=(("al_prac", number))  sort="screen A " )

		em_mail_audit.user_id,   
		em_mail_audit.mail_from,   
		em_mail_audit.mail_to,   
		em_mail_audit.mail_cc,   
		em_mail_audit.mail_bcc,   
		em_mail_audit.mail_subject,   
		em_mail_audit.mail_date,   
		em_mail_audit.mail_importance,   
		em_mail_audit.mail_attach_name,   
		em_mail_audit.ctx_id_list,   
		em_mail_audit.doc_id_list,   
		em_mail_audit.ai_id_list,   
		em_mail_audit.wf_id,   
		em_mail_audit.alarm_type,   
		em_mail_audit.field_name,   
		em_mail_audit.send_result,   
		em_mail_audit.error_text,   
		em_mail_audit.notes,   
		em_mail_audit.mail_template_id,
		em_mail_audit.compute_info,
		company_name = app_facility.facility_name	
	FROM em_mail_audit   
	Left Outer Join ctx_basic_info on em_mail_audit.ctx_id_list = convert(varchar(100),ctx_basic_info.ctx_id)
	Left Outer Join app_facility on ctx_basic_info.app_facility = app_facility.facility_id"  sort="mail_date D " )

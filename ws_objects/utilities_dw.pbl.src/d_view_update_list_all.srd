         systable.creator,   
         systable.table_name,   
         systable.view_def,   
         systable.table_type,   
         sysobjects.id  
    FROM systable,   
         sysobjects  
   WHERE ( systable.table_name = sysobjects.name ) and  
         ( systable.table_name like 'v_%' ) AND  
         systable.creator = 1 AND  
         sysobjects.type = 'V'    

 retrieve="  SELECT qa_measurement_new.profile_group_measurement_id,   
         qa_measurement_new.qa_profile_id,   
         qa_measurement_new.threshold_a,   
         qa_measurement_new.threshold_e,   
         qa_measurement_new.include_in_profile,   
         qa_measurement_new.data_format,   
         qa_measurement_new.qa_metric_id,   
         qa_measurement_new.category_id,   
         qa_measurement_new.qa_measurement_name,   
         qa_measurement_new.category_name,   
         qa_metrics.indicator  
    FROM qa_measurement_new,   
         qa_metrics  
   WHERE ( qa_measurement_new.qa_metric_id = qa_metrics.qam_id ) and  
         ( qa_measurement_new.qa_profile_id = :ai_profile ) AND  
         (( qa_measurement_new.include_in_profile = 1 ) OR  
         ( qa_measurement_new.include_in_profile is null ) )    
" update="qa_measurement_new" updatewhere=0 updatekeyinplace=no arguments=(("ai_profile", number))  sort="category_name A " )

 retrieve="  SELECT DISTINCT qa_measurement_new.qa_measurement_name,   
         qa_measurement_new.qa_profile_id,   
         qa_profile.profile_name,   
         qa_measurement_new.data_format,   
         qa_measurement_new.category_name,   
         pd_qa_profile_data.pd_qap_id,   
         pd_qa_profile_data.pd_qa_hdr_id,   
         pd_qa_profile_data.profile_measurement_id,   
         pd_qa_profile_data.stored_value,   
         qa_metrics.indicator,   
         qa_measurement_new.qa_metric_id  
    FROM qa_measurement_new,   
         qa_profile,   
         pd_qa_profile_data,   
         qa_metrics  
   WHERE ( qa_measurement_new.qa_profile_id = qa_profile.qa_profile_id ) and  
         ( pd_qa_profile_data.profile_measurement_id = qa_measurement_new.profile_group_measurement_id ) and  
         ( qa_measurement_new.qa_metric_id = qa_metrics.qam_id ) and  
         ( (  qa_measurement_new.qa_profile_id = :al_profile_id ) AND  
         ( qa_measurement_new.profile_group_measurement_id in ( :al_mets ) ) )    
" update="pd_qa_profile_data" updatewhere=0 updatekeyinplace=yes arguments=(("al_mets", numberlist),("al_profile_id", number))  sort="category_name A " )

 retrieve="  SELECT pd_qa_hdr.active_status,   
         pd_qa_hdr.to_date,   
         pd_qa_hdr.from_date,   
         pd_qa_hdr.prac_id,   
         pd_qa_hdr.qa_profile_id,   
         pd_qa_hdr.pd_qa_hdr_id,   
         pd_basic.first_name,   
         pd_basic.last_name,   
         qa_profile.profile_name,   
         pd_qa_hdr.hdr_comment,   
         pd_qa_hdr.reason,   
         pd_qa_hdr.facility_id as facility_id
    FROM pd_qa_hdr,   
         pd_basic,   
         qa_profile  
   WHERE ( pd_qa_hdr.prac_id = pd_basic.prac_id ) and  
         ( pd_qa_hdr.qa_profile_id = qa_profile.qa_profile_id ) and  
         ( pd_qa_hdr.prac_id = :prac_id )     and
		( pd_qa_hdr.facility_id = :facil_id)
" arguments=(("prac_id", number),("facil_id", number))  sort="pd_basic_last_name A from_date A " )

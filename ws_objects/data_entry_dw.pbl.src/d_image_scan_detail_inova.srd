         pd_images.prac_id,   
         pd_images.facility_id,   
         pd_images.seq_no,   
         pd_images.screen_id,   
         pd_images.date_scanned,   
         pd_images.user_name,   
         pd_images.date_recieved,   
         pd_images.active_status,   
         pd_images.doc_id,   
         pd_images.notes,   
         pd_images.num_pages,   
         pd_images.reference_name,   
         pd_images.tif_start_page,   
         pd_images.image_type_id,   
         selected=0,   
         pd_images.de_queue  ,
		pd_images.image_pk,
		pd_images.image_ext
    FROM pd_images  
   WHERE ( pd_images.de_queue = 0 ) AND
         ( pd_images.active_status = 1 ) AND    
			( pd_images.prac_id = :prac_id )
" update="pd_images" updatewhere=0 updatekeyinplace=no arguments=(("prac_id", number)) )

 retrieve="  SELECT distinct wf_workflow.wf_name as wf_name,
         wf_action_types.action_desc as step_name,   
         convert(varchar(12), net_dev_action_items.rec_id) as doc_id,
         net_dev_action_items.prac_id as prac_id,
	    net_dev_action_items.wf_id as wf_id,
	    net_dev_action_items.app_id as app_id,
         net_dev_action_items.wf_complete_flag as wf_complete_flag,
		wf_type = 'A'
    FROM net_dev_action_items,    
         wf_workflow,   
         wf_action_types  
   WHERE ( wf_action_types.wf_id = wf_workflow.wf_id ) and  
         ( wf_workflow.wf_id = net_dev_action_items.wf_id ) and  
          ( net_dev_action_items.wf_id > 0 )  AND
		 ( net_dev_action_items.prac_id = :al_prac )

" arguments=(("al_prac", number))  sort="wf_name A wf_id A " )

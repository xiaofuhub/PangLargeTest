 retrieve="  SELECT Top 1 v_full_name.full_name,   
         v_full_name.date_of_birth,   
         v_full_name.gender,   
         v_full_name.npi_number,   
         v_mailing_address.e_mail_address,   
         v_dept.dept_name,   
		v_affil_staff_cat_active.staff_cat,
         v_primary_specialty.specialty_name,	
		pd_affil_stat.apptmnt_start_date ,
		pd_affil_stat.apptmnt_end_date,
		pd_affil_stat.prior_appt_start ,
		pd_affil_stat.prior_appt_end,
		pd_affil_stat.inital_appmnt_date,
		pd_affil_stat.priority_user,
		v_full_name.ssn,
		v_full_name.provider_id,
		pd_basic.car_phone_1,
		pd_basic.mobile_carrier,
		code_lookup.custom_1,
		code_lookup.description,
		cust_1 = '',
		cust_2 = '',
		cust_3 = ''		
    FROM v_full_name LEFT OUTER JOIN v_mailing_address ON v_full_name.prac_id = v_mailing_address.prac_id and   v_mailing_address.facility_id = :ai_facil
 LEFT OUTER JOIN v_primary_specialty ON v_full_name.prac_id = v_primary_specialty.prac_id  ,
pd_affil_stat LEFT OUTER JOIN v_dept ON pd_affil_stat.rec_id = v_dept.rec_id and v_dept.primary_dept = 1  
 LEFT OUTER JOIN v_affil_staff_cat_active ON pd_affil_stat.rec_id = v_affil_staff_cat_active.rec_id and v_affil_staff_cat_active.requested = 1,
pd_basic LEFT OUTER JOIN code_lookup ON pd_basic.mobile_carrier = code_lookup.lookup_code
   WHERE   
			pd_affil_stat.prac_id  = v_full_name.prac_id and
			pd_basic.prac_id  = v_full_name.prac_id and
			v_full_name.prac_id = :prac_id and 
			 pd_affil_stat.parent_facility_id = :ai_facil and
pd_affil_stat.active_status in (1,4)		;
" arguments=(("prac_id", number),("ai_facil", number)) )

 retrieve="  SELECT DISTINCT pd_comm_review.rec_id,   
         pd_comm_review.prac_id,   
         pd_comm_review.date_sent_for_review,   
         pd_comm_review.date_back_from_review,   
         pd_comm_review.review_results,   
         pd_comm_review.facility_id,   
         pd_comm_review.seq_no,   
         pd_comm_review.active_status,   
         pd_comm_review.committee_id,   
         Editable = 1,   
         security_user_facilities.access_rights,   
         pd_comm_review.meeting_id,   
         pd_comm_review.approved_user  ,
         meet_sel = pd_comm_review.meeting_id,
         committee_lookup.sort_order,
		icred_settings.set_web
    FROM pd_comm_review,   
         security_user_facilities ,
         committee_lookup ,
		icred_settings
   WHERE ( pd_comm_review.facility_id = security_user_facilities.facility_id ) and  
         ( pd_comm_review.rec_id = :rec_id ) AND  
         ( pd_comm_review.committee_id = committee_lookup.committee_id) and
         security_user_facilities.user_id = :as_user    
" update="pd_comm_review" updatewhere=0 updatekeyinplace=no arguments=(("rec_id", number),("as_user", string))  sort="committee_lookup_sort_order A date_sent_for_review A committee_id A " )

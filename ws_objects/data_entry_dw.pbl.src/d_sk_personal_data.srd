 retrieve="  SELECT DISTINCT v_full_name.full_name,   
         v_full_name.date_of_birth,   
         v_full_name.gender,   
         v_full_name.npi_number,  
         v_primary_specialty.specialty_name, 
         v_mailing_address.e_mail_address  
    FROM  v_full_name LEFT OUTER JOIN v_mailing_address ON v_full_name.prac_id = v_mailing_address.prac_id and   v_mailing_address.facility_id = :ai_facil 
 LEFT OUTER JOIN v_primary_specialty ON v_full_name.prac_id = v_primary_specialty.prac_id 
   WHERE ( v_full_name.prac_id = :prac_id )
        

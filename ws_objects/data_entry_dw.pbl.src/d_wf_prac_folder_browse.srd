 retrieve="  SELECT DISTINCT facility.facility_name,   
         wf_workflow.wf_name,   
         app_hdr.application_name,   
         net_dev_action_items.app_id,   
         net_dev_action_items.prac_id,   
         net_dev_action_items.wf_id,   
         net_dev_action_items.facility_id  
    FROM facility,   
         net_dev_action_items,   
         app_hdr,   
         wf_workflow  
   WHERE (net_dev_action_items.facility_id =  facility.facility_id ) and  
         ( net_dev_action_items.app_id = app_hdr.app_id ) and  
         ( net_dev_action_items.wf_id = wf_workflow.wf_id ) and  
          ( net_dev_action_items.prac_id = :ai_prac ) AND  
         ( net_dev_action_items.wf_id > 0 )     
" arguments=(("ai_prac", number)) )

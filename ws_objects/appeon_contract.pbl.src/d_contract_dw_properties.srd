         ctx_screen.screen_id,   
         isnull(ctx_screen.titlebar_visible, 'Y' ) titlebar_visible,   
         ctx_screen.titlebar_text,   
         isnull( ctx_screen.boarder_style, 'stylelowered!' )boarder_style,   

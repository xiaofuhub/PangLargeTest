 retrieve="  SELECT wv_letter.wv_letter_id,
              wv_letter.letter_name,
              wv_letter.document_name,
              wv_letter.letter_desc,
              wv_letter.letter_type_id,
              active_status = wv_letter_criterion_active.field_value,
              '              '  doc_file,
              standing_status = wv_letter_criterion_standing.field_value,
			data_len = datalength(letter_file)
    from   wv_letter ,
             (select * from wv_letter_criterion where field_name = 'pd_affil_stat.active_status' ) wv_letter_criterion_active ,
             (select * from wv_letter_criterion where field_name = 'pd_affil_stat.in_good_standing' ) wv_letter_criterion_standing
  where wv_letter.wv_letter_id = wv_letter_criterion_active.wv_letter_id
      and wv_letter.wv_letter_id = wv_letter_criterion_standing.wv_letter_id
" )

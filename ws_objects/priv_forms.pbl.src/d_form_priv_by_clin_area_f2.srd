 retrieve="  SELECT top 1 facility.facility_name,   
         priv_clinical_area.clinical_area_code,   
         priv_clinical_area.clinical_area_description,   
         priv_core.priv_core_description,   
         priv_core_procd.sort_order,   
         priv_core_procd.procd_name,   
         priv_core.priv_core_id,   
         priv_core.sort_order,   
         facility.street,   
         facility.street_2,   
         facility.city,   
         facility.state,   
         facility.zip,   
         facility.phone,   
         facility.fax,   
         facility.return_facility_name,   
         priv_template.header_text,   
         priv_template.excl_cont_text,   
         priv_clinical_area.use_exclusive  
    FROM facility,   
         priv_clinical_area,   
         priv_core,   
         priv_core_procd,   
         priv_template  
   WHERE ( priv_core.clinical_area_id = priv_clinical_area.clinical_area_id ) and  
         ( priv_core_procd.priv_core_id = priv_core.priv_core_id ) and  
         ( facility.priv_template = priv_template.template_id ) and  
         ( priv_template.template_id = priv_clinical_area.facility_id ) and  
         ( ( facility.facility_id = :ai_facility ) AND  
         ( priv_clinical_area.clinical_area_id = :ai_clin_area ) AND  
         ( priv_core_procd.active_status = 1 ) )   
ORDER BY facility.facility_name ASC,   
         priv_clinical_area.clinical_area_code ASC,   
         priv_core.priv_core_description ASC,   
         priv_core_procd.sort_order ASC   

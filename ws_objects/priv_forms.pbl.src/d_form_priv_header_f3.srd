         priv_clinical_area.clinical_area_description,   
         facility.street,   
         facility.street_2,   
         facility.city,   
         facility.state,   
         facility.zip,   
         priv_template.header_text,   
         priv_template.excl_cont_text,   
         priv_clinical_area.use_exclusive  
    FROM facility,   
         priv_clinical_area,   
         priv_template  
   WHERE ( priv_template.template_id = facility.priv_template ) and  
         ( priv_clinical_area.facility_id = priv_template.template_id ) and  
         ( ( priv_clinical_area.clinical_area_id = :ai_clin_id ) AND  

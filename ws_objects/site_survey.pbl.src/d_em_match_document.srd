 column=(type=decimal(0) updatewhereclause=yes name=em_mail_items_mail_action_item_id dbname="em_mail_items.mail_action_item_id" )
 column=(type=char(10) updatewhereclause=yes name=em_mail_items_mail_save_format dbname="em_mail_items.mail_save_format" )
 retrieve="select 
em_mail_items.id,
em_mail_items.mail_subject,
em_mail_items.mail_from,
em_mail_items.mail_to,
em_mail_items.mail_cc,
em_mail_items.mail_attach_name,
em_mail_items.mail_date,
em_mail_items.mail_ctx_id,
em_mail_items.mail_doc_id,
em_mail_items.mail_action_item_id,
em_mail_items.mail_save_format
from em_mail_items 
left outer join ctx_am_document on em_mail_items.mail_doc_id = ctx_am_document.doc_id
where em_mail_items.mail_doc_id = :al_doc_id and
	em_mail_items.mail_attach_name <> ''and
	em_mail_items.mail_attach_name is not null and
	em_mail_items.folder_id = 2 and deleted = 0 and mail_fromclx <> 2 and
	lower(REPLACE(em_mail_items.mail_attach_name,'.',' ')) Like Lower('%' + REPLACE(:as_attachname,'.',' ') + ' (%')
order by em_mail_items.mail_date desc

" arguments=(("al_doc_id", number),("as_attachname", string)) )

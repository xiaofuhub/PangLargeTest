 retrieve="  SELECT em_mail_items.id,   
         em_mail_items.user_id,   
         em_mail_items.folder_id,   
         em_mail_items.mail_msg_id,   
         em_mail_items.mail_date,   
         em_mail_items.mail_subject,   
         em_mail_items.mail_importance,   
         em_mail_items.mail_from,   
         em_mail_items.mail_to,   
         em_mail_items.mail_cc,   
         em_mail_items.mail_ctx_id,   
			em_mail_items.ctx_id_list, 
			em_mail_items.doc_id_list, 
         em_mail_items.mail_doc_id,   
         em_mail_items.mail_action_item_id,   
         em_mail_items.mail_fromclx,   
         em_mail_items.read_status,   
         em_mail_items.flag,   
         em_mail_items.notes,   
         em_mail_items.locked,   
         em_mail_items.deleted,   
         em_mail_items.mail_attach_name,   
         em_mail_items.mail_save_format,   
         em_mail_items.sub_folder_id,   
         em_mail_items.last_modify_date  ,
		em_mail_items.mail_type,
		em_mail_items.mail_format,
			folder_display = Case em_mail_items.deleted when 1 then -1 else em_mail_items.sub_folder_id end,
			checkin_status  = 
         ( Select Top 1 1 From ctx_am_document,  (Select Top 1 doc_name_format From icred_settings) a 
				Where ctx_am_document.doc_id = em_mail_items.mail_doc_id 
				And ctx_am_document.checkout_by = em_mail_items.user_id
				And ctx_am_document.checkout_status <> 0 
				And em_mail_items.mail_attach_name <> ''
				And (Case a.doc_name_format
						When 0 Then CharIndex(ctx_am_document.doc_name + '.' +  ctx_am_document.doc_ext + ' (' ,em_mail_items.mail_attach_name) 
						When 1 Then CharIndex(ctx_am_document.doc_name + ' ' + Convert(varchar,ctx_am_document.doc_id) + '.' 
													 + Convert(varchar,ctx_am_document.revision) + '.' +  ctx_am_document.doc_ext + ' (' 
													 ,em_mail_items.mail_attach_name )
					End ) > 0
			), 
		protocoltype
    FROM em_mail_items
" update="em_mail_items" updatewhere=0 updatekeyinplace=no  sort="mail_date D " )

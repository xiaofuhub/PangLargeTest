 column=(type=char(15) updatewhereclause=yes name=code dbname="code" )
 retrieve="  SELECT pd_address.rec_id,
			pd_address.street,   
         pd_address.street_2,   
         pd_address.city,   
         pd_address.zip,   
         pd_address.phone,   
         code_lookup_a.code,   
         code_lookup_b.code  
    FROM {oj {oj pd_address  LEFT OUTER JOIN code_lookup code_lookup_a  ON pd_address.state = code_lookup_a.lookup_code} LEFT OUTER JOIN code_lookup code_lookup_b  ON pd_address.county = code_lookup_b.lookup_code}  
   WHERE ( pd_address.street like :name ) AND  
         ( pd_address.city like :city ) AND  
         ( code_lookup_a.code like :state )    
" arguments=(("name", string),("city", string),("state", string)) )

table(column=(type=char(255) updatewhereclause=yes name=screen_name dbname="ctx_screen.screen_name" dbalias=".screen_name" )
 column=(type=long updatewhereclause=yes name=screen_id dbname="ctx_screen.screen_id" dbalias=".screen_id" )
 column=(type=char(200) updatewhereclause=yes name=dataobject dbname="dataobject" )
 retrieve="  SELECT ctx_screen.screen_name,   
         ctx_screen.screen_id,   
          ( case when screen_style = 'G' then  ctx_screen.grid_dataobject else  ctx_screen.dataobject  end ) dataobject  
    FROM ctx_screen  
   WHERE ( ctx_screen.data_view_id = :ai_view_id ) AND  
         ( ctx_screen.screen_id not between 34 and 41 ) AND  
         ( ctx_screen.screen_id not in (13,15,16,23,25,27,28,29,2,58 ) )   
ORDER BY ctx_screen.screen_name ASC   
" arguments=(("ai_view_id", number))  sort="screen_name A " )

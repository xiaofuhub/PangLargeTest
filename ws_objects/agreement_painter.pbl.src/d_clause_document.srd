 retrieve="  SELECT Distinct ctx_am_document.doc_id,   
         ctx_am_document.ctx_id,   
         ctx_am_document.revision,   
         ctx_am_document.doc_ext,   
         ctx_am_document.category,   
         ctx_am_document.export_id,   
         ctx_am_document.status,   
         ctx_am_document.checkout_status,   
         ctx_am_document.revision_control, 
  			ctx_am_document.create_by, 
         checked = 0,   
         ctx_am_document.doc_name,   
         ctx_basic_info.category,   
         ctx_basic_info.app_facility,   
         ctx_basic_info.status,   
         contract_category = 	code_lookup_a.code ,   
         contract_status = 	code_lookup_b.code, 
			document_category = 	code_lookup_c.code, 
			document_status = 	code_lookup_d.code,
         app_facility.facility_name  ,
			ctx_am_doc_image.modify_date,
			update_result = space(100)
    FROM ctx_basic_info 
			LEFT OUTER JOIN code_lookup code_lookup_a ON ctx_basic_info.category = code_lookup_a.lookup_code 
			LEFT OUTER JOIN code_lookup code_lookup_b ON ctx_basic_info.status = code_lookup_b.lookup_code 
			LEFT OUTER JOIN app_facility ON ctx_basic_info.app_facility = app_facility.facility_id,   
         ctx_am_document
			LEFT OUTER JOIN code_lookup code_lookup_c ON ctx_am_document.category = code_lookup_c.lookup_code 
			LEFT OUTER JOIN code_lookup code_lookup_d ON ctx_am_document.status = code_lookup_d.lookup_code ,   
         ctx_am_document_clause, 
			ctx_am_doc_image 
   WHERE ( ctx_am_document.doc_id = ctx_am_document_clause.doc_id ) and  
         ( ctx_am_document.revision = ctx_am_document_clause.revision ) and  

			( ctx_am_document.doc_id = ctx_am_doc_image.doc_id ) and  
         ( ctx_am_document.revision = ctx_am_doc_image.revision ) and  

         ( ctx_basic_info.ctx_id = ctx_am_document.ctx_id ) and  
         ( ( ctx_am_document_clause.ctx_acp_clause_id In (:al_clause_id) ) )    
" arguments=(("al_clause_id", numberlist))  sort="ctx_am_document_ctx_id A ctx_am_document_doc_name A " )

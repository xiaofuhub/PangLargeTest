         sum(ctx_acp_template_clause_rules.rule_row_id)  
    FROM ctx_acp_template_clause INNER  JOIN ctx_acp_template_clause_rules ON (ctx_acp_template_clause.ctx_acp_template_id = ctx_acp_template_clause_rules.ctx_acp_template_id AND ctx_acp_template_clause.ctx_acp_clause_id = ctx_acp_template_clause_rules.ctx_acp_clause_id),   
         ctx_acp_clause  
   WHERE ( ctx_acp_clause.ctx_acp_clause_id = ctx_acp_template_clause.ctx_acp_clause_id ) and  
         ( ( ctx_acp_template_clause.ctx_acp_template_id = :template_id ) )   and ctx_acp_template_clause_rules.data_view_id = :as_dataviewid

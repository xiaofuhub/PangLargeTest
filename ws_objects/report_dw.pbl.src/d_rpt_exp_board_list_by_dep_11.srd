table(column=(type=decimal(0) updatewhereclause=yes name=pd_board_specialty_rec_id dbname="pd_board_specialty.rec_id" )
 column=(type=char(218) updatewhereclause=yes name=v_full_name_full_name dbname="v_full_name.full_name" )
 column=(type=decimal(0) updatewhereclause=yes name=v_full_name_prac_id dbname="v_full_name.prac_id" )
 column=(type=char(75) updatewhereclause=yes name=code_lookup_description dbname="code_lookup.description" )
 column=(type=char(100) updatewhereclause=yes name=address_lookup_entity_name dbname="address_lookup.entity_name" )
 column=(type=char(75) updatewhereclause=yes name=code_lookup_department dbname="code_lookup.department" )
 column=(type=char(30) updatewhereclause=yes name=dep dbname="code_lookup.dep" )
 column=(type=datetime updatewhereclause=yes name=pd_board_specialty_expiration_date dbname="pd_board_specialty.expiration_date" )
 retrieve="  SELECT distinct pd_board_specialty.rec_id,   
         v_full_name.full_name,
   		v_full_name.prac_id,
         code_lookup_a.description,   
         address_lookup.entity_name,  
         code_lookup_b.description as department,   
         code_lookup_b.code as dep, 
         pd_board_specialty.expiration_date  
    FROM pd_board_specialty,
			pd_spec_facility_link,
			address_lookup,
         code_lookup code_lookup_a,   
         v_full_name,   
         pd_affil_stat,  
         code_lookup code_lookup_b,
         pd_affil_dept  
   WHERE ( pd_board_specialty.prac_id = v_full_name.prac_id ) and
			( pd_board_specialty.rec_id = pd_spec_facility_link.specialty_id ) and   
         ( pd_board_specialty.specialty = code_lookup_a.lookup_code ) and  
         ( pd_board_specialty.board_code = address_lookup.lookup_code ) and  
         ( v_full_name.prac_id = pd_affil_stat.prac_id ) and 
   (  pd_affil_stat.rec_id = pd_affil_dept.rec_id ) and 
         (  pd_board_specialty.board_code is not null ) AND  
         ( pd_spec_facility_link.practicing = 581 ) AND      
			( pd_spec_facility_link.facility_id = pd_affil_stat.parent_facility_id) and 
         ( pd_affil_dept.department = code_lookup_b.lookup_code ) and  
			( pd_board_specialty.expiration_date -30 <= {fn curdate()} )  

"  sort="v_full_name_full_name A " )

 column=(type=char(100) updatewhereclause=yes name=address_lookup_entity_name dbname="address_lookup.entity_name" )
 column=(type=datetime updatewhereclause=yes name=pd_board_specialty_expiration_date dbname="pd_board_specialty.expiration_date" )
 retrieve="  SELECT distinct pd_board_specialty.rec_id,   
         v_full_name.full_name,
   		v_full_name.prac_id,
         code_lookup.description,   
         address_lookup.entity_name,   
         pd_board_specialty.expiration_date
    FROM pd_board_specialty,
   		pd_spec_facility_link,
         address_lookup,   
         code_lookup,   
         v_full_name,   
         pd_affil_stat  
   WHERE ( pd_board_specialty.prac_id = v_full_name.prac_id ) and
			( pd_board_specialty.rec_id = pd_spec_facility_link.specialty_id ) and   
         ( pd_board_specialty.specialty = code_lookup.lookup_code ) and  
         ( pd_board_specialty.board_code = address_lookup.lookup_code ) and  
         ( v_full_name.prac_id = pd_affil_stat.prac_id ) and  
         ( ( pd_board_specialty.board_code is not null ) AND  
         ( pd_spec_facility_link.practicing = 581 )   ) 
 
"  sort="v_full_name_full_name A " )

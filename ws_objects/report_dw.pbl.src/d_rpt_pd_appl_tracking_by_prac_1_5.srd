         sys_appl_tracker.step_7_desc,   
         sys_appl_tracker.step_8_desc,   
         sys_appl_tracker.step_9_desc,   
         sys_appl_tracker.step_10_desc,   
         sys_appl_tracker.tracker_id,   
         sys_appl_tracker.step_1_type,   
         sys_appl_tracker.step_2_type,   
         sys_appl_tracker.step_3_type,   
         sys_appl_tracker.step_4_type,   
         sys_appl_tracker.step_5_type,   
         sys_appl_tracker.step_6_type,   
         sys_appl_tracker.step_7_type,   
         sys_appl_tracker.step_8_type,   

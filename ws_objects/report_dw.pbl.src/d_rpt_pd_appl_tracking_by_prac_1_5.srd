         sys_appl_tracker.final_step,   
         pd_appl_tracking.active_status,   
         pd_appl_tracking.assigned_user  
    FROM pd_appl_tracking left outer join group_practice on pd_appl_tracking.gp_id = group_practice.rec_id  ,   
         v_full_name,   
         app_hdr,   
         facility,   
         sys_appl_tracker  
   WHERE ( pd_appl_tracking.prac_id = v_full_name.prac_id ) and  
         ( pd_appl_tracking.app_id = app_hdr.app_id ) and  
         ( pd_appl_tracking.facility_id = facility.facility_id ) and 
         ( pd_appl_tracking.active_status = 1) AND   
         ( ( pd_appl_tracking.facility_id = :ai_facil ) AND  
         ( pd_appl_tracking.step_1 >= :ad_from ) AND  
         ( pd_appl_tracking.step_1 <= :ad_to ) )    

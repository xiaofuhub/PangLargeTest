 retrieve="  SELECT ctx_am_action_item.doc_id,
         ctx_am_action_item.seq_id,
         ctx_am_action_item.action_order,
         ctx_am_action_item.action_type,
         ctx_am_action_item.action_user,
         ctx_am_action_item.action_status,
         ctx_am_action_item.status_date,
         ctx_am_action_item.due_date,
         ctx_am_action_item.attempt_times,
         ctx_am_action_item.first_attempt,
         ctx_am_action_item.last_attempt,
         ctx_am_action_item.print_flag,
         ctx_am_action_item.create_date,
         ctx_am_action_item.letter_id,
         ctx_am_action_item.wf_step,
         ctx_am_action_item.wf_id,
         ctx_am_action_item.wf_action_type_id,
         ctx_am_action_item.wf_assigned_by,
         ctx_am_action_item.active_status,
         ctx_am_action_item.wf_complete_flag,
         ctx_am_action_item.notes,
         ctx_am_action_item.reminder_last,
         ctx_am_action_item.escalation_last,
         ctx_am_action_item.custom_n1,
         ctx_am_action_item.custom_n2,
         ctx_am_action_item.custom_n3,
         ctx_am_action_item.custom_n4,
         ctx_am_action_item.custom_n5,
         ctx_am_action_item.custom_1,
         ctx_am_action_item.custom_2,
         ctx_am_action_item.custom_3,
         ctx_am_action_item.custom_4,
         ctx_am_action_item.priority,
         ctx_am_action_item.subject,
         ctx_am_action_item.start_date,
         ctx_am_action_item.start_time,
         ctx_am_action_item.duration,
         ctx_am_action_item.duration_type,
         ctx_am_action_item.end_date,
         ctx_am_action_item.end_time,
         wf_workflow.wf_module,
         ctx_am_document.ctx_id
    FROM ctx_am_action_item Left Outer Join wf_workflow On  ( ctx_am_action_item.wf_id = wf_workflow.wf_id) ,
         ctx_am_document  
   WHERE 
         ( ctx_am_action_item.doc_id = ctx_am_document.doc_id) AND
         ( ( ctx_am_action_item.doc_id = :al_doc_id ) AND
         ( ctx_am_action_item.seq_id = :al_seq_id ) )

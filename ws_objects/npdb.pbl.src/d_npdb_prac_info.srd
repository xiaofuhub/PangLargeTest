 retrieve="  SELECT pd_basic.prac_id,   
         pd_basic.first_name,   
         pd_basic.last_name,   
         ISNULL(pd_basic.middle_name, ''),   
         pd_basic.title,   
         pd_basic.name_suffix,   
         pd_basic.prac_category,   
         pd_basic.date_of_birth,   
         pd_basic.ssn,   
         pd_basic.sex  ,
			GENDER  = (SELECT top 1 CODE_LOOKUP.CODE FROM CODE_LOOKUP WHERE CODE_LOOKUP.LOOKUP_CODE = pd_basic.sex ),
         license = isnull((select ic_n from CODE_LOOKUP  where CODE_LOOKUP.LOOKUP_CODE = (SELECT TOP 1 PD_LICENSE.LICENSE_TYPE_CODE
                                                                                           FROM  PD_LICENSE 
                                                                                           WHERE PD_LICENSE.PRAC_ID =pd_basic.prac_id )  ),'010'),
		   pd_education.SCHOOL_CODE,
		   license_number = isnull((select top 1 license_number from pd_license  where pd_license.prac_id = pd_basic.prac_id), 'NO LICENSE'),
			school_name = isnull((select entity_name from address_lookup where address_lookup.lookup_code = pd_education.SCHOOL_CODE),''),
         pd_education.year_thru,
         pd_education.month_thru,
         STATE_OF_LICENsE = (SELECT CODE FROM CODE_LOOKUP WHERE CODE_LOOKUP.LOOKUP_CODE = (SELECT TOP 1 state from pd_license where pd_license.prac_id = pd_basic.prac_id ) )

  FROM  pd_basic LEFT OUTER JOIN pd_education ON pd_basic.prac_id = pd_education.prac_id
  WHERE pd_basic.prac_id in( :AL_PRAC_ID ) 
"
  arguments=(("AL_PRAC_ID", numberlist))
)

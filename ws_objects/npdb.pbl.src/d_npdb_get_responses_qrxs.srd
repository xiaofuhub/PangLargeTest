		 npdb_qrxs_hdr.doc_id,
		npdb_qrxs_hdr.prac_id,
		 npdb_qrxs_individual_total.last_name,
		 npdb_qrxs_individual_total.first_name,
		 npdb_qrxs_status.process_date,
		 npdb_qrxs_status.report_count, 
		 verif_info.response_code
FROM npdb_qrxs_hdr, npdb_qrxs_individual_total, npdb_qrxs_status, 
		verif_info, security_user_facilities 
WHERE ( npdb_qrxs_hdr.npdb_hdr_id = npdb_qrxs_individual_total.npdb_hdr_id ) and
           (npdb_qrxs_individual_total.report_id =  npdb_qrxs_hdr.report_id ) and

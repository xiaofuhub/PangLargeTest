         pd_basic.ssn ,
			prac_id = pd_basic.prac_id 
    FROM {oj pd_basic  LEFT OUTER JOIN code_lookup code_lookup_a  ON pd_basic.name_suffix = code_lookup_a.lookup_code},   
         code_lookup code_lookup_b  
